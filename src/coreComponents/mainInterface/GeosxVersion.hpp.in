/**
 * @file GeosxVersion.hpp
 *
 * GEOSX version file.
 * Contains a CMake-generated list of macros that define a particular software version.
 */

#ifndef GEOS_MAININTERFACE_VERSION_HPP_
#define GEOS_MAININTERFACE_VERSION_HPP_

/// GEOS major version number
#define GEOS_VERSION_MAJOR @GEOS_VERSION_MAJOR@

/// GEOS minor version number
#define GEOS_VERSION_MINOR @GEOS_VERSION_MINOR@

/// GEOS patch version number
#define GEOS_VERSION_PATCH @GEOS_VERSION_PATCH@

/// GEOS full version number string
#define GEOS_VERSION_FULL  "@GEOS_VERSION_FULL@"

/// GEOS development branch string
#cmakedefine GEOS_GIT_BRANCH "@GEOS_GIT_BRANCH@"

/// GEOS development branch string
#cmakedefine GEOS_GIT_HASH "@GEOS_GIT_HASH@"

/// GEOS development branch string
#cmakedefine GEOS_GIT_TAG "@GEOS_GIT_TAG@"

#endif  /* GEOS_MAININTERFACE_VERSION_HPP_ */

