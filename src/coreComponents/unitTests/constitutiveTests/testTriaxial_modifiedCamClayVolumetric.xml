<?xml version="1.0" ?>

<Problem>
  <Tasks>
    <TriaxialDriver
      name="triaxialDriver"
      material="MCC"
      mode="strainControl"
      axialControl="strainFunction"
      radialControl="strainFunction"
      initialStress="-1.0"
      steps="50"
      logLevel="0"
      baseline="testTriaxial_modifiedCamClayVolumetric.txt"
      output="none"/>
  </Tasks>

  <Events
    maxTime="1">
    <SoloEvent
      name="triaxialDriver"
      target="/Tasks/triaxialDriver"/>
  </Events>

  <Constitutive>

    <ModifiedCamClay
      name="MCC"
      defaultDensity="2700"
      defaultRefPressure="-1.0"
      defaultRefStrainVol="0"
      defaultShearModulus="200.0"
      defaultPreConsolidationPressure="-1.1"
      defaultCslSlope="1.2"
      defaultVirginCompressionIndex="0.003"
      defaultRecompressionIndex="0.002"/>

  </Constitutive>

  <Functions>
    <TableFunction
      name="strainFunction"
      inputVarNames="{ time }"
      coordinates="{ 0.0, 1.0 }"
      values="{ 0, -0.005}"/>
  </Functions>

  <!-- Mesh is not used, but GEOSX throws an error without one.  Will resolve soon-->
  <Mesh>
    <InternalMesh
      name="mesh1"
      elementTypes="{ C3D8 }"
      xCoords="{ 0, 1 }"
      yCoords="{ 0, 1 }"
      zCoords="{ 0, 1 }"
      nx="{ 1 }"
      ny="{ 1 }"
      nz="{ 1 }"
      cellBlockNames="{ cellBlock01 }"/>
  </Mesh>

</Problem>
