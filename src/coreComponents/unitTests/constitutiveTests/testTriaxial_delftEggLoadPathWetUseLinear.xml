<?xml version="1.0" ?>

<Problem>
  <Tasks>
    <TriaxialDriver
      name="triaxialDriver"
      material="DE"
      mode="mixedControl" 
      axialControl="strainFunction"
      radialControl="stressFunction"
      initialStress="-1.0"
      steps="50"
      logLevel="0"
      baseline ="testTriaxial_delftEggLoadPathWetUseLinear.txt"
      output="none" />
  </Tasks>

  <Events
    maxTime="1">
    <SoloEvent
      name="triaxialDriver"
      target="/Tasks/triaxialDriver"/>
  </Events>

  <Constitutive>

    <DelftEgg
      name="DE"
      defaultDensity="2700"
      defaultBulkModulus="500.0"
      defaultShearModulus="200.0"
      defaultPreConsolidationPressure="-2.0"
      defaultShapeParameter="1.8"
      defaultCslSlope="1.2"
      defaultVirginCompressionIndex="0.003"
      defaultRecompressionIndex="0.002"/>

  </Constitutive>

  <Functions>
    <TableFunction
      name="strainFunction"
      inputVarNames="{ time }"
      coordinates="{ 0.0, 1.0 }"
      values="{ 0, -0.008}"/>
    <TableFunction
      name="stressFunction"
      inputVarNames="{ time }"
      coordinates="{ 0.0, 1.0 }"
      values="{ -1.0, -1.0 }"/>
  </Functions>

  <!-- Mesh is not used, but GEOSX throws an error without one.  Will resolve soon-->
  <Mesh>
    <InternalMesh
      name="mesh1"
      elementTypes="{ C3D8 }"
      xCoords="{ 0, 1 }"
      yCoords="{ 0, 1 }"
      zCoords="{ 0, 1 }"
      nx="{ 1 }"
      ny="{ 1 }"
      nz="{ 1 }"
      cellBlockNames="{ cellBlock01 }"/>
  </Mesh>

</Problem>
