<?xml version="1.0" ?>
<!--# # -->
<Problem xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="geos_v0.0.xsd">

<Parameters>  
  <Parameter name="Parameter0" value="0.0" />
  <Parameter name="Parameter1" value="1.0" />
  <Parameter name="Parameter2" value="2.0" />
  <Parameter name="Parameter3" value="3.0" />
</Parameters>

<Solvers>
    <Hydrofracture name="solver1" 
                   timeIntegration="Explicit"
                   ppSolverName="ppsolve"
                   lgSolverName="lagsolve"/>

    <LagrangeSmallStrainLinearElastic name="lagsolve" 
                  courant="0.5" 
                  timeIntegrationOption="2"
                  dampingM="200"
                  gravityVector="0 0 -9.81e-3"/>

    <ParallelPlateFlowSolver name="ppsolve"
              BulkModulus="3e8" 
              mu="0.01"
              BartonJointParameters="5e-4 5e6 1e-4"
              pressurecap="120e6"
              MaximumAperture="30.0e-3"
              scalingOption="1"
              maxViscosity="1e3"
              propSolverName="propsolve"
              gravityVector="0 0 -9.81">
          <LeakoffModel CartersLeakoffCoefficient="2.5e-13" 
                        pressureDependentLeakoff="1" 
                        referenceViscosity="0.001" 
                        minDeltaT="10.0"/>
     </ParallelPlateFlowSolver>

    <!-- diameter="0.273e-3" -->
      <ProppantTransportSolver name="propsolve"
                               tol="1e-10" 
                               scalingOption="1" 
                               maxFluxCoefficient="0.9"
                               explicitPack="0"
                               courant="0.5"
                               Diffusivity="1e-6"
                               flowSolver="ppsolve"
                               fixedFaceVf="0"
                               gravityVector="0 0 -9.81">

                     <Proppant density="2650"
                               fluidDensity="1000"
                               fluidViscosity="0.001"
                               diameter="0.004"
                               maxPackingFraction="0.6"
                               gravity="9.81"
                               bridgingFactor="1.0">

                  <SlurryModel singleParticleSettlingModel="stokes"
                               hinderedSettlingCoefficient="-5.9"
                               beta="1.5"/>
                    </Proppant>
  </ProppantTransportSolver>

</Solvers>

	<Mesh xcoords="0 40" 
	      ycoords="-10 10"
        zcoords="-10 10"
	      nx="20"
	      ny="10"
	      nz="10"
	      elementType="C3D8" regionNames="Region" trianglePattern="0"
	      skewAngle="0"
	      />
	      
	<SolverApplications>
    <Application name="1" begintime="0.0" endtime="4.0" dt="0.002">
			<Apply solver="solver1" toregions="Region" />
		</Application>
	</SolverApplications>

  <Events maxTime="4.0">
    <PeriodicEvent name="solverApplications"
                   forceDt="0.002"
                   target="/Solvers/solver1" />

    <PeriodicEvent name="outputs"
                   timeFrequency="0.1"
                   targetExactTimestep="1"
                   target="/Outputs/siloOutput" />
  </Events>

  <ElementRegions>
  	<ElementRegion name="Region" elementtype="poly" >
          <LinearElasticMaterial ShearModulus="4.16667e9" Density="200000" BulkModulus="5.5556e9" />
      </ElementRegion>
  </ElementRegions>

	<FieldSpecifications>
    <!-- Explicit BC's -->
    <BoundaryCondition  object="Node" fieldname="Velocity" setnames="xneg xpos" component="0" scale="0.0" fieldtype="Vector" />
    <BoundaryCondition  object="Node" fieldname="Velocity" setnames="yneg ypos" component="1" scale="0.0" fieldtype="Vector" />
    <BoundaryCondition  object="Node" fieldname="Velocity" setnames="zneg zpos" component="2" scale="0.0" fieldtype="Vector" />
  	
    <!-- Flow BC's -->
    <BoundaryCondition object="Face" fieldname="combinedFlowRate" setnames="source" scale="1.0"/>
    <!-- <BoundaryCondition object="Face" fieldname="ProppantVolumeFraction" setnames="source" scale="0.001" timetable="ttable2"/> -->
    <BoundaryCondition object="Face" fieldname="ProppantVolumeFraction" setnames="source" scale="1e-9" timetable="ttable2"/>
	</FieldSpecifications>
	
  
  <Tables>

    <Table1D name="ttable2" 
             coord="0, 1.0, 1.000001, 1e6"
             value="0.0, 0.0, 1.0, 1.0" />
  </Tables>

  <Outputs>
    <Silo name="siloOutput" plotFileRoot="plot" writeFEMEdges="0" writeFEMFaces="1" writePlot="1" writeRestart="0"/>
  </Outputs>

	<Nodesets>
    <Nodeset name="all" type="0" xmin="-1e6 -1e6 -1e6" xmax="1e6 1e6 1e6" />
    <Nodeset name="source" type="0" xmin="-0.1 -0.01 -0.1" xmax="2.1 0.01 2.1" />
    <Nodeset name="Frac_0" type="0" xmin="-0.1 -0.01 -4.1" xmax="4.1 0.01 4.1" />
    <Nodeset name="fracturable" type="0" xmin="-0.1 -0.01 -8.1" xmax="1000.1 0.01 8.1" />
	</Nodesets>

  <Partition>
    <!-- <SpatialPartition xpar="2" ypar="2" zpar="2" /> -->
  </Partition>
	
	<Fracture fractureFlag="100" 
            preFractureSetName="source Frac_0"
            separableSet="fracturable"
            rockToughness="1e6" 
            failCriterion="1"
            failgap="0.0004"
            dfnPrefix="Frac_" />

</Problem>



