<?xml version="1.0" ?>

<Problem>
  <Included>
    <File name="./PoroElastic_efem-edfm_verticalFrac_base.xml" />
  </Included>

  <Geometry>
    <BoundedPlane
      name="FracturePlane"
      normal="{ 1, 0, 0 }"
      origin="{ 5.0, 10.0, 0.5 }"
      lengthVector="{ 0, 1, 0 }"
      widthVector="{ 0, 0, 1 }"
      dimensions="{ 12, 4 }"/>
  </Geometry>

  <Outputs>
    <VTK
      name="vtkOutput"
      plotFileRoot="PoroElastic_efem-edfm_verticalFrac"
      plotLevel="2"
      format="binary"/>
    <TimeHistory
      name="pressureHistoryOutput"
      sources="{/Tasks/pressureCollection}"
      filename="pressure_history"/> 
    <TimeHistory
      name="apertureOutput"
      sources="{/Tasks/apertureCollection}"
      filename="aperture_history"/> 
    <Restart
      name="restartOutput"/>
  </Outputs>
</Problem>