<?xml version="1.0" ?>

<Problem>
  <Included>
    <File name="./PoroElastic_Mandel_base.xml"/>
  </Included>  

  <!-- SPHINX_MESH -->
  <Mesh>
    <InternalMesh
      name="mesh1"
      elementTypes="{ C3D8 }"
      xCoords="{ 0.0, 1.0 }"
      yCoords="{ 0.0, 0.1 }"
      zCoords="{ 0.0, 1.0 }"
      nx="{ 10 }"
      ny="{ 1 }"
      nz="{ 10 }"
      cellBlockNames="{ cb1 }"/>
  </Mesh>
  <!-- SPHINX_MESH_END -->

  <Events
    maxTime="0.1">
  
     <PeriodicEvent
      name="outputs"
      timeFrequency="5.0e-01"
      target="/Outputs/vtkOutput"/>

    <PeriodicEvent
      name="solverApplication0"
      beginTime="0.0"
      endTime="10.0"
      forceDt="0.025"
      target="/Solvers/poroSolve"/> 

    <PeriodicEvent
      name="pressureCollection" 
      target="/Tasks/pressureCollection"/>

    <PeriodicEvent
      name="pressureOutput"
      targetExactTimestep="0"
      target="/Outputs/pressureHistoryOutput"/>

    <PeriodicEvent
      name="displacementCollection" 
      target="/Tasks/displacementCollection" />

    <PeriodicEvent
      name="displacementOutput"
      targetExactTimestep="0" 
      target="/Outputs/displacementHistoryOutput" />   
  </Events>
 
</Problem>
