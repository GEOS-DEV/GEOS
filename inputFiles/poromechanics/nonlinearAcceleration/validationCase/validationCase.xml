  </Geometry>

  <ElementRegions>
    <CellElementRegion name="extendedRegion"
                  cellBlocks="{ cb-0_0_0, cb-1_0_0, cb-2_0_0, cb-0_1_0, cb-1_1_0, cb-2_1_0, cb-0_2_0, cb-1_2_0, cb-2_2_0,
                                cb-0_0_1, cb-1_0_1, cb-2_0_1, cb-0_1_1,           cb-2_1_1, cb-0_2_1, cb-1_2_1, cb-2_2_1,
