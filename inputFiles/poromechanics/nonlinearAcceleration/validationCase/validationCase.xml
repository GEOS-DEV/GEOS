<Problem>
  <Solvers
    gravityVector="{ 0.0, 0.0, 0.0 }">

    <!-- SPHINX_POROMECHANICSSOLVER -->
    <SinglePhasePoromechanics
      name="singlePhasePoroelasticity"
      solidSolverName="linearElasticity"
      flowSolverName="singlePhaseFlow"
      logLevel="1"
      targetRegions="{ reservoir, extendedRegion }"
      useNonlinearAcceleration="1">
      <NonlinearSolverParameters
        newtonMaxIter="200"
        newtonTol="1.0e-6"
        couplingType="Sequential"
        lineSearchAction="None"
        subcycling="1"/>
      <LinearSolverParameters
        solverType="gmres"
        preconditionerType="mgr"
        logLevel="1"
        krylovTol="1.0e-6"
        krylovMaxIter="100"/>
    </SinglePhasePoromechanics>
    <!-- SPHINX_POROMECHANICSSOLVER_END -->

    <!-- SPHINX_MECHANICALSOLVER -->
    <SolidMechanicsLagrangianSSLE
      name="linearElasticity"
      timeIntegrationOption="QuasiStatic"
      logLevel="1"
      discretization="FE1"
      targetRegions="{ reservoir, extendedRegion }">
      <NonlinearSolverParameters
        newtonTol="1.0e-7"
        newtonMaxIter="40"/>
      <LinearSolverParameters
        solverType="gmres"
        preconditionerType="amg"
        krylovTol="1.0e-6"
        logLevel="1"/>
    </SolidMechanicsLagrangianSSLE>      
    <!-- SPHINX_MECHANICALSOLVER_END -->

    <!-- SPHINX_SINGLEPHASEFVM -->
    <SinglePhaseFVM
      name="singlePhaseFlow"
      logLevel="1"
      discretization="singlePhaseTPFA"
      targetRegions="{ reservoir, extendedRegion }"
      temperature="380.372">
      <NonlinearSolverParameters
        newtonTol="1.0e-7"
        newtonMaxIter="40"/>
      <LinearSolverParameters
        solverType="gmres"
        preconditionerType="amg"
        krylovTol="1.0e-6"
        logLevel="1"/>
    </SinglePhaseFVM>
    <!-- SPHINX_SINGLEPHASEFVM_END -->

  </Solvers>

  <!-- SPHINX_NUMERICAL -->
  <NumericalMethods>
    <FiniteElements>
      <FiniteElementSpace
        name="FE1"
        order="1"/>
    </FiniteElements>

    <FiniteVolume>
      <TwoPointFluxApproximation
        name="singlePhaseTPFA"/>
    </FiniteVolume>
  </NumericalMethods>
  <!-- SPHINX_NUMERICAL_END -->

  <!-- SPHINX_MESH -->
  <Mesh>
    <InternalMesh
      name="mesh"
      elementTypes="{ C3D8 }"
      xCoords="{ 0, 6096, 12801.6, 18897.6 }"
      yCoords="{ 0, 3048, 6400.8, 9448.8 }"
      zCoords="{ -3185.16, -3124.2, -3048, -2987.04, -2743.2, -2133.6, -1219.2, 0 }"
      nx="{ 5, 11, 5 }"
      ny="{ 5, 11, 5 }"
      nz="{ 2, 5, 1, 1, 1, 1, 1 }"
      cellBlockNames="{ b00,b01,b02,b03,b04,b05,b06,
                        b07,b08,b09,b10,b11,b12,b13,
                        b14,b15,b16,b17,b18,b19,b20, 
                        b21,b22,b23,b24,b25,b26,b27,
                        b28,b29,b30,b31,b32,b33,b34,
                        b35,b36,b37,b38,b39,b40,b41,
                        b42,b43,b44,b45,b46,b47,b48,
                        b49,b50,b51,b52,b53,b54,b55,
                        b56,b57,b58,b59,b60,b61,b62 }">
    </InternalMesh>

  </Mesh>
  <!-- SPHINX_MESH_END -->

  <Geometry>
  <!-- SPHINX_INJ_SOURCE -->
    <Box
      name="source"
      xMin="{ 9143.9, 4571.9, -3093.82 }"
      xMax="{ 9753.7, 4876.9, -3078.38 }"/>

  <!-- SPHINX_INJ_SOURCE_END -->

    <Box
      name="corner0"
      xMin="{ -0.01, -0.01, -0.01 }"
      xMax="{ 0.01, 0.01, 0.01 }"/>
    <Box
      name="corner1"
      xMin="{ 18897.59, -0.01, -0.01 }"
      xMax="{ 18897.61, 0.01, 0.01 }"/>
    <Box
      name="corner2"
      xMin="{ -0.01, 9448.79, -0.01 }"
      xMax="{ 0.01, 9448.81, 0.01 }"/>
    <Box
      name="corner3"
      xMin="{ 18897.59, 9448.79, -0.01 }"
      xMax="{ 18897.61, 9448.81, 0.01 }"/>

    <Box
      name="bottomEdge0"
      xMin="{ -0.01, -0.01, -3185.17 }"
      xMax="{ 0.01, 9448.81, -3185.15 }"/>
    <Box
      name="bottomEdge1"
      xMin="{ 18897.59, -0.01, -3185.17 }"
      xMax="{ 18897.61, 9448.81, -3185.15 }"/>
    <Box
      name="bottomEdge2"
      xMin="{ -0.01, -0.01, -3185.17 }"
      xMax="{ 18897.61, 0.01, -3185.15 }"/>
    <Box
      name="bottomEdge3"
      xMin="{ -0.01, 9448.79, -3185.17 }"
      xMax="{ 18897.61, 9448.81, -3185.15 }"/>

    <Box
      name="verticalEdge0"
      xMin="{ -0.01, -0.01, -3185.18 }"
      xMax="{ 0.01, 0.01, 0.01 }"/>
    <Box
      name="verticalEdge1"
      xMin="{ 18897.59, -0.01, -3185.18 }"
      xMax="{ 18897.61, 0.01, 0.01 }"/>
    <Box
      name="verticalEdge2"
      xMin="{ -0.01, 9448.79, -3185.18 }"
      xMax="{ 0.01, 9448.81, 0.01 }"/>
    <Box
      name="verticalEdge3"
      xMin="{ 18897.59, 9448.79, -3185.18 }"
      xMax="{ 18897.61, 9448.81, 0.01 }"/>

  </Geometry>

  <ElementRegions>
    <CellElementRegion name="extendedRegion"
                  cellBlocks="{ b00,b01,b02,b03,b04,b05,b06,
                                b07,b08,b09,b10,b11,b12,b13,
                                b14,b15,b16,b17,b18,b19,b20, 
                                b21,b22,b23,b24,b25,b26,b27,
                                b28,    b30,b31,b32,b33,b34,
                                b35,b36,b37,b38,b39,b40,b41,
                                b42,b43,b44,b45,b46,b47,b48,
                                b49,b50,b51,b52,b53,b54,b55,
                                b56,b57,b58,b59,b60,b61,b62 }"
                  materialList="{fluid, porousRockExt}"/>
    <CellElementRegion name="reservoir"
                  cellBlocks="{ b29 }"
                  materialList="{ fluid, porousRockRes }"/>


  </ElementRegions>

  <Functions>                                                                                                                                                                                                                                                                                                                                                
    <TableFunction                                                                                                                                                               
      name="statusTable"                                                                                                                                                         
      inputVarNames="{ time }"                                                                                                                                                   
      interpolation="lower"                                                                                                                                                      
      coordinates="{ -1e11, 0, 1e11 }"                                                                                                                                           
      values="{ -1, 1, 1 }"/>                                                                                                                                                                                                                                                                                                                                 
  </Functions>   

  <Events
      minTime="-1e11"
      maxTime="8640000">
   
    <PeriodicEvent
      name="outputs"
      beginTime="0"
      timeFrequency="1728000"
      target="/Outputs/vtkOutput"/>  

    <PeriodicEvent
      name="solverApplication"
      beginTime="0"
      forceDt="1728000"
      target="/Solvers/singlePhasePoroelasticity"/>

    <PeriodicEvent
      name="timeHistoryCollection"
      beginTime="0"
      timeFrequency="1728000"
      target="/Tasks/pressureCollection"/>

    <PeriodicEvent
      name="timeHistoryOutput"
      beginTime="0"
      timeFrequency="1728000"
      targetExactTimestep="0"
      target="/Outputs/timeHistoryOutput"/>

  </Events>

  <Constitutive>
    <CompressibleSinglePhaseFluid
      name="fluid"
      defaultDensity="1000"
      defaultViscosity="0.001"
      referencePressure="101325"
      referenceDensity="1000"
      compressibility="4.496e-10"
      referenceViscosity="0.001"
      viscosibility="0.0"/>

    <PorousElasticIsotropic
      name="porousRockRes"
      solidModelName="skeletonRes"
      porosityModelName="rockPorosityRes"
      permeabilityModelName="rockPermRes"/>
    <ElasticIsotropic
      name="skeletonRes"
      defaultDensity="2700"
      defaultYoungModulus="6.894757e+7"
      defaultPoissonRatio="0.25"/>
    <ConstantPermeability
      name="rockPermRes"
      permeabilityComponents="{ 9.869233e-14, 9.869233e-14, 9.869233e-15 }"/>
    <BiotPorosity
      name="rockPorosityRes"
      grainBulkModulus="1.0e27"
      defaultReferencePorosity="0.25"/>

    <PorousElasticIsotropic
      name="porousRockExt"
      solidModelName="skeletonExt"
      porosityModelName="rockPorosityExt"
      permeabilityModelName="rockPermExt"/>
    <ElasticIsotropic
      name="skeletonExt"
      defaultDensity="2700"
      defaultYoungModulus="6.894757e+9"
      defaultPoissonRatio="0.25"/>
    <ConstantPermeability
      name="rockPermExt"
      permeabilityComponents="{ 0, 0, 0 }"/>
    <BiotPorosity
      name="rockPorosityExt"
      grainBulkModulus="1.0e27"
      defaultReferencePorosity="0.25"/>

  </Constitutive>


  <FieldSpecifications>

    <FieldSpecification
      name="initialPressure"
      initialCondition="1"
      setNames="{ all }"
      objectPath="ElementRegions"
      fieldName="pressure"
      scale="2.7579e+7"/>

    <FieldSpecification
      name="xconstraint"
      objectPath="nodeManager"
      fieldName="totalDisplacement"
      component="0"
      scale="0.0"
      setNames="{ xneg, xpos }"/>

    <FieldSpecification
      name="yconstraint"
      objectPath="nodeManager"
      fieldName="totalDisplacement"
      component="1"
      scale="0.0"
      setNames="{ yneg, ypos }"/>

    <FieldSpecification
      name="zconstraint"
      objectPath="nodeManager"
      fieldName="totalDisplacement"
      component="2"
      scale="0.0"
      setNames="{ zneg }"/>

    <FieldSpecification
      name="xCornersConstraint"
      objectPath="nodeManager"
      fieldName="totalDisplacement"
      component="0"
      scale="0.0"
      setNames="{ corner0, corner1, corner2, corner3 }"/>
    <FieldSpecification
      name="yCornersConstraint"
      objectPath="nodeManager"
      fieldName="totalDisplacement"
      component="1"
      scale="0.0"
      setNames="{ corner0, corner1, corner2, corner3 }"/>
    <FieldSpecification
      name="zCornersConstraint"
      objectPath="nodeManager"
      fieldName="totalDisplacement"
      component="2"
      scale="0.0"
      setNames="{ corner0, corner1, corner2, corner3 }"/>

    <FieldSpecification
      name="xVerticalEdgesConstraint"
      objectPath="nodeManager"
      fieldName="totalDisplacement"
      component="0"
      scale="0.0"
      setNames="{ verticalEdge0, verticalEdge1, verticalEdge2, verticalEdge3 }"/>
    <FieldSpecification
      name="yVerticalEdgesConstraint"
      objectPath="nodeManager"
      fieldName="totalDisplacement"
      component="1"
      scale="0.0"
      setNames="{ verticalEdge0, verticalEdge1, verticalEdge2, verticalEdge3 }"/>

    <FieldSpecification
      name="xXZBottomEdgesConstraint"
      objectPath="nodeManager"
      fieldName="totalDisplacement"
      component="0"
      scale="0.0"
      setNames="{ bottomEdge0, bottomEdge1 }"/>
    <FieldSpecification
      name="zXZBottomEdgesConstraint"
      objectPath="nodeManager"
      fieldName="totalDisplacement"
      component="2"
      scale="0.0"
      setNames="{ bottomEdge0, bottomEdge1 }"/>

    <FieldSpecification
      name="yYZBottomEdgesConstraint"
      objectPath="nodeManager"
      fieldName="totalDisplacement"
      component="1"
      scale="0.0"
      setNames="{ bottomEdge1, bottomEdge2 }"/>
    <FieldSpecification
      name="zYZBottomEdgesConstraint"
      objectPath="nodeManager"
      fieldName="totalDisplacement"
      component="2"
      scale="0.0"
      setNames="{ bottomEdge1, bottomEdge2 }"/>

    <Traction
      name="zposconstraint"
      objectPath="faceManager"
      tractionType="normal"
      scale="-2.7579e+7"
      setNames="{ zpos }"/>

    <FieldSpecification
      name="sigma_xx_Res"
      initialCondition="1"
      setNames="{ all }"
      objectPath="ElementRegions/reservoir/b29"
      fieldName="skeletonRes_stress"
      component="0"
      scale="0.0"/>
    <FieldSpecification
      name="sigma_xx_Ext"
      initialCondition="1"
      setNames="{ all }"
      objectPath="ElementRegions/extendedRegion"
      fieldName="skeletonExt_stress"
      component="0"
      scale="0.0"/>
    <FieldSpecification
      name="sigma_yy_Res"
      initialCondition="1"
      setNames="{ all }"
      objectPath="ElementRegions/reservoir/b29"
      fieldName="skeletonRes_stress"
      component="1"
      scale="0.0"/>
    <FieldSpecification
      name="sigma_yy_Ext"
      initialCondition="1"
      setNames="{ all }"
      objectPath="ElementRegions/extendedRegion"
      fieldName="skeletonExt_stress"
      component="1"
      scale="0.0"/>
    <FieldSpecification
      name="sigma_zz_Res"
      initialCondition="1"
      setNames="{ all }"
      objectPath="ElementRegions/reservoir/b29"
      fieldName="skeletonRes_stress"
      component="2"
      scale="0.0"/>
    <FieldSpecification
      name="sigma_zz_Ext"
      initialCondition="1"
      setNames="{ all }"
      objectPath="ElementRegions/extendedRegion"
      fieldName="skeletonExt_stress"
      component="2"
      scale="0.0"/>

    <FieldSpecification
      name="xInitialTotalDisplacement"
      initialCondition="1"
      objectPath="nodeManager"
      fieldName="totalDisplacement"
      component="0"
      scale="0.0"
      setNames="{ all }"/>
    <FieldSpecification
      name="yInitialTotalDisplacement"
      initialCondition="1"
      objectPath="nodeManager"
      fieldName="totalDisplacement"
      component="1"
      scale="0.0"
      setNames="{ all }"/>
    <FieldSpecification
      name="zInitialTotalDisplacement"
      initialCondition="1"
      objectPath="nodeManager"
      fieldName="totalDisplacement"
      component="2"
      scale="0.0"
      setNames="{ all }"/>

    <SourceFlux
      name="sourceTerm"
      objectPath="ElementRegions/reservoir/b29"
      scale="-1.84E+01"
      setNames="{ source }"/>

  </FieldSpecifications>

  <Tasks>
    <SolidMechanicsStatistics
      name="linearElasticityStatistics"
      solidSolverName="linearElasticity"
      logLevel="1"/>
    <SinglePhaseStatistics
      name="singlePhaseFlowStatistics"
      flowSolverName="singlePhaseFlow"
      logLevel="1"/>

    <PackCollection
      name="pressureCollection"
      objectPath="ElementRegions/reservoir/b29"
      fieldName="pressure"/>

  </Tasks>

  <Outputs>
    <VTK
      name="vtkOutput"/>

    <TimeHistory
      name="timeHistoryOutput"
      sources="{ /Tasks/pressureCollection }"
      filename="pressure_history"/>

    <Restart
      name="restartOutput"/>
  </Outputs>

</Problem>

