<?xml version="1.0" ?>

<Problem>
  <Solvers gravityVector="{0.0, 0.0, 0.0}">
    <LagrangianContact
      name="fractureMechSolver"
      solidSolverName="mechanicsSolver"
      stabilizationName="singlePhaseTPFA_stab"
      logLevel="1"
      targetRegions="{ Reservoir, Overburden, Underburden, Fault}" 
      fractureRegionName="Fault"
      discretization="FE1"
      contactRelationName="fractureContact">
      <NonlinearSolverParameters
        maxTimeStepCuts="1"
        newtonTol="1e0"
        newtonMaxIter="10"
        maxAllowedResidualNorm="1e+15"/>
      <LinearSolverParameters
        directParallel="0"/> 
    </LagrangianContact>   

    <SolidMechanicsLagrangianSSLE 
      name="mechanicsSolver" 
      timeIntegrationOption="QuasiStatic"
      logLevel="1"
      discretization="FE1"
      targetRegions="{ Reservoir, Overburden, Underburden }" >
      <NonlinearSolverParameters
      maxTimeStepCuts="1"
      newtonTol="1e0"
      newtonMaxIter="10"
      maxAllowedResidualNorm="1e+15"/>
    <LinearSolverParameters
      directParallel="0"/> 
    </SolidMechanicsLagrangianSSLE>

    <SurfaceGenerator
      logLevel="0"
      name="SurfaceGen"
      targetRegions="{ Reservoir, Overburden, Underburden }" 
      fractureRegion="Fault"
      rockToughness="1e6"
      mpiCommOrder="1"/>
  </Solvers>

  <Mesh>
    <InternalMesh
    name="mesh1"
    elementTypes="{ C3D8 }"
    xCoords="{ -2250, 0, 2250 }"
    yCoords="{ -2250, -150, -75, 75, 150, 2250 }"
    zCoords="{ 0, 500 }"
    nx="{ 60, 60 }"
    ny="{ 56, 2, 4, 2, 56 }"
    nz="{ 2 }"
    cellBlockNames="{ cb1, cb2, cb3, cb4, cb5, cb6, cb7, cb8, cb9, cb10 }"/>
  </Mesh> 

  <ElementRegions>
    <CellElementRegion
      name="Reservoir"
      cellBlocks="{ cb2, cb3, cb8, cb9 }"
      materialList="{ reservoirRock }"/>
    <CellElementRegion
      name="Overburden"
      cellBlocks="{  cb4, cb5, cb10 }"
      materialList="{ burdenRock }"/> 
    <CellElementRegion
      name="Underburden"
      cellBlocks="{ cb1, cb6, cb7 }"
      materialList="{ burdenRock }"/>       

  <SurfaceElementRegion
      name="Fault"
      materialList="{ fractureContact }"
      defaultAperture="1e-3"/>
  </ElementRegions> 

  <Geometry>
    <Box
    name="fault"
    xMin="{ -0.01,  -150.01,   -0.01 }"
    xMax="{ 0.01,    150.01,   500.01 }"/>
    <Box
    name="faultReservoir"
    xMin="{ -0.01,  -150.01,   -0.01 }"
    xMax="{ 0.01,    150.01,   500.01 }"/>
    <Box
    name="bottomFace"
    xMin="{ -2250.01,  -2250.01,   -0.01 }"
    xMax="{ 2250.01,   -2249.01,   500.01 }"/>
    <Box
    name="topFace"
    xMin="{ -2250.01,  2249.01,   -0.01 }"
    xMax="{ 2250.01,   2250.01,   500.01 }"/>

    <Box
    name="zposFace"
    xMin="{ -2250.01,  -2250.01,  499.99 }"
    xMax="{ 2250.01,   2250.01,   500.01 }"/>

    <Box
    name="znegFace"
    xMin="{ -2250.01,  -2250.01,   -0.01 }"
    xMax="{ 2250.01,   2250.01,     0.01 }"/>
  </Geometry>

  <NumericalMethods>
    <FiniteVolume>
      <TwoPointFluxApproximation
        name="singlePhaseTPFA_stab"/>  
    </FiniteVolume>
    <FiniteElements>
      <FiniteElementSpace
        name="FE1"
        order="1"/>
    </FiniteElements>
  </NumericalMethods>

  <Constitutive>
    <ElasticIsotropic
      name="reservoirRock"
      defaultDensity="2650.0"
      defaultYoungModulus="14.95e9"
      defaultPoissonRatio="0.15"/>

    <ElasticIsotropic
      name="burdenRock"
      defaultDensity="2650.0"
      defaultYoungModulus="14.95e9"
      defaultPoissonRatio="0.15"/>  
      
    <Coulomb
      name="fractureContact"
      cohesion="0.0"
      frictionCoefficient="0.5"
      apertureTableName="apertureTable"/>

  </Constitutive>

  <FieldSpecifications>
    <FieldSpecification
      name="frac"
      initialCondition="1"
      setNames="{ fault }"
      objectPath="faceManager"
      fieldName="ruptureState"
      scale="1"/>

    <FieldSpecification
      name="separableFace"
      initialCondition="1"
      setNames="{ fault }"
      objectPath="faceManager"
      fieldName="isFaceSeparable"
      scale="1"/>
    	     
    <FieldSpecification
      name="xconstraint"
      component="0"
      setNames="{ xneg, xpos }"
      objectPath="nodeManager"
      fieldName="totalDisplacement"
      scale="0.0"/>

    <FieldSpecification
      name="yconstraint"
      component="1"
      setNames="{ yneg }"
      objectPath="nodeManager"
      fieldName="totalDisplacement"
      scale="0.0"/>

    <FieldSpecification
      name="zconstraint"
      objectPath="nodeManager"
      fieldName="totalDisplacement"
      component="2"
      scale="0.0"
      setNames="{ zneg, zpos }"/>

    <FieldSpecification 
      name="initialStress1"
      initialCondition="1"
      setNames="{ all }"
      objectPath="ElementRegions/Reservoir"
      fieldName="reservoirRock_stress"
      component="0"
      scale="-3.0e6"/>
      
    <FieldSpecification 
      name="initialStress2"
      initialCondition="1"
      setNames="{ all }"
      objectPath="ElementRegions/Reservoir"
      fieldName="reservoirRock_stress"
      component="1"
      scale="-3.0e6"/>
      
    <FieldSpecification 
      name="initialStress3"
      initialCondition="1"
      setNames="{ all }"
      objectPath="ElementRegions/Reservoir"
      fieldName="reservoirRock_stress"
      component="2"
      scale="-3.0e6"/>  

    <FieldSpecification 
      name="OverburdenStress1"
      initialCondition="1"
      setNames="{ all }"
      objectPath="ElementRegions/Overburden"
      fieldName="burdenRock_stress"
      component="0"
      scale="-4.0e6"/>
      
    <FieldSpecification 
      name="OverburdenStress2"
      initialCondition="1"
      setNames="{ all }"
      objectPath="ElementRegions/Overburden"
      fieldName="burdenRock_stress"
      component="1"
      scale="-4.0e6"/>
      
    <FieldSpecification 
      name="OverburdenStress3"
      initialCondition="1"
      setNames="{ all }"
      objectPath="ElementRegions/Overburden"
      fieldName="burdenRock_stress"
      component="2"
      scale="-4.0e6"/>  

   <FieldSpecification 
      name="UnderburdenStress1"
      initialCondition="1"
      setNames="{ all }"
      objectPath="ElementRegions/Underburden"
      fieldName="burdenRock_stress"
      component="0"
      scale="-4.0e6"/>
      
    <FieldSpecification 
      name="UnderburdenStress2"
      initialCondition="1"
      setNames="{ all }"
      objectPath="ElementRegions/Underburden"
      fieldName="burdenRock_stress"
      component="1"
      scale="-4.0e6"/>
      
    <FieldSpecification 
      name="UnderburdenStress3"
      initialCondition="1"
      setNames="{ all }"
      objectPath="ElementRegions/Underburden"
      fieldName="burdenRock_stress"
      component="2"
      scale="-4.0e6"/>  

    <Traction
      name="tractionTop"
      objectPath="faceManager"
      tractionType="normal"
      scale="-4.0e6"      
      setNames="{ ypos }"/>
      
  </FieldSpecifications>

  <Functions>
    <TableFunction
      name="apertureTable"
      coordinates="{ -1.0e-3, 0.0 }"
      values="{ 1.0e-3, 1.0e-3 }"/>
  </Functions>

  <Outputs>
    <VTK 
      name="vtkOutput"
      parallelThreads="1"
      plotFileRoot="verticalFault"/>

    <Restart
      name="restartOutput"/>  
  </Outputs>

  <Events maxTime="1000.0">
    <SoloEvent
      name="preFracture"
      target="/Solvers/SurfaceGen"/>

    <PeriodicEvent
      name="outputs"
      cycleFrequency="1"
      target="/Outputs/vtkOutput"/>

    <PeriodicEvent
      name="solverApplications"
      forceDt="1000"
      target="/Solvers/fractureMechSolver"/>

    <!-- <PeriodicEvent
      name="solverApplications"
      forceDt="1000"
      target="/Solvers/mechanicsSolver"/>   -->
      
      
  </Events>

</Problem>
