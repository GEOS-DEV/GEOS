<?xml version="1.0" ?>

<Problem>
  <Included>
    <File
      name="./PoroElastic_conformingFracture_2d_openingFrac_base.xml"/>
  </Included> 

  <Mesh>
    <InternalMesh
      name="mesh1"
      elementTypes="{ C3D8 }"
      xCoords="{ -1, 1 }"
      yCoords="{ -1, 1 }"
      zCoords="{ 0, 1 }"
      nx="{ 2 }"
      ny="{ 4 }"
      nz="{ 1 }"
      cellBlockNames="{ cb1 }"/>
  </Mesh>

  <Events
    maxTime="1.0">
    <SoloEvent
      name="preFracture"
      target="/Solvers/SurfaceGen"/>

    <SoloEvent
      name="preFractureplot"
      target="/Outputs/vtkOutput"/>

    <PeriodicEvent
      name="solverApplications"
      forceDt="1.0"
      target="/Solvers/poroSolver"/>

    <PeriodicEvent
      name="outputs"
      cycleFrequency="1"
      target="/Outputs/vtkOutput"/>

    <!-- <PeriodicEvent
      name="restart"
      target="/Outputs/restarts"/>   -->
  </Events>

</Problem>
