<?xml version="1.0" ?>

<Problem>
  <Included name="primary_includes">
    <File
      name="./PoroElastic_conformingFracture_2d_faultSlip_solvers.xml"/>
  </Included>
  <Included name="secondary_includes">
    <File
      name="./PoroElastic_conformingFracture_2d_faultSlip_base.xml"/>
  </Included>

  <Mesh>
    <InternalMesh
      name="mesh1"
      elementTypes="{ C3D8 }"
      xCoords="{ -100, -20, 20, 100 }"
      yCoords="{ -100, -5, 5, 100 }"
      zCoords="{ 0, 1 }"
      nx="{ 1, 2, 1 }"
      ny="{ 1, 2, 1 }"
      nz="{ 1 }"
      cellBlockNames="{ cb1 }"/>
  </Mesh>

  <Events
    maxTime="1000.0">
    <SoloEvent
      name="preFracture"
      target="/Solvers/SurfaceGen"/>

    <SoloEvent
      name="preFractureplot"
      target="/Outputs/vtkOutput"/>

    <PeriodicEvent
      name="solverApplications"
      forceDt="1000.0"
      target="/Solvers/poroSolver"/>

    <PeriodicEvent
      name="outputs"
      cycleFrequency="1"/>

    <PeriodicEvent
      name="restarts"
      cycleFrequency="1"
      target="/Outputs/restarts"/>
  </Events>

</Problem>