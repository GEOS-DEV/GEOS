<?xml version="1.0" ?>

<Problem>
  <Included>
    <File
      name="./PoroElastic_conformingFracture_2d_faultSlip_base.xml"/>
  </Included> 

  <Mesh>
    <InternalMesh
      name="mesh1"
      elementTypes="{ C3D8 }"
      xCoords="{ -100, -20, 20, 100 }"
      yCoords="{ -100, -5, 5, 100 }"
      zCoords="{ 0, 1 }"
      nx="{ 20, 40, 20 }"
      ny="{ 10, 10, 10 }"
      nz="{ 1 }"
      cellBlockNames="{ cb1 }"/>
  </Mesh>

  <Events
    maxTime="1000.0">
    <SoloEvent
      name="preFracture"
      target="/Solvers/SurfaceGen"/>

    <SoloEvent
      name="preFractureplot"
      target="/Outputs/vtkOutput"/>

    <PeriodicEvent
      name="solverApplications"
      forceDt="100.0"
      target="/Solvers/poroSolver"/>

    <PeriodicEvent
      name="outputs"
      cycleFrequency="1"
      target="/Outputs/vtkOutput"/>
  </Events>

</Problem>