<?xml version="1.0" ?>

<Problem>
  <Included>
    <File name="./TFrac_base.xml"/>
  </Included>

  <Solvers
    gravityVector="{0.0, 0.0, 0.0}">
 <SurfaceGenerator
   name="SurfaceGen"
   logLevel="0"
   fractureRegion="Fracture"
   targetRegions="{ Region }"
   initialRockToughness="1.0e6"
   mpiCommOrder="1"/>
<!-- SPHINX_SOLVER_END -->
</Solvers>

  <NumericalMethods>
    <FiniteElements>
      <FiniteElementSpace
        name="FE1"
        order="1"/>
    </FiniteElements>
  </NumericalMethods>

  <!-- SPHINX_MESH -->
  <Mesh>
    <InternalMesh
      name="mesh1"
      elementTypes="{ C3D8 }"
      xCoords="{ -1000, -100, 100, 1000 }"
      yCoords="{ -1000, -100, 100, 1000 }"
      zCoords="{ 0, 100 }"
      nx="{ 20, 20, 20 }"
      ny="{ 20, 20, 20 }"
      nz="{ 2 }"
      cellBlockNames="{ cb1 }"/>
  </Mesh>
  <!-- SPHINX_MESH_END -->
</Problem>
