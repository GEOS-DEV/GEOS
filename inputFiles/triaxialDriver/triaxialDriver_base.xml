<?xml version="1.0" ?>

<Problem>
  <!-- SPHINX_EVENTS -->
  <Events
    maxTime="1">
    <SoloEvent
      name="triaxialDriver"
      target="/Tasks/triaxialDriver"/>
  </Events>
  <!-- SPHINX_EVENTS_END -->

  <!-- SPHINX_MATERIAL -->
  <Constitutive>  
	<!-- SPHINX_MATERIAL_DRUCKER_PRAGER --> 
    <DruckerPrager 
      name="DruckerPrager"
      defaultDensity="2700"
      defaultBulkModulus="10.0e9"
      defaultShearModulus="6.0e9"
      defaultCohesion="0.1e6"
      defaultFrictionAngle="6.0"
      defaultDilationAngle="3.0"
      defaultHardeningRate="0.5e9"
    /> 
	<!-- SPHINX_MATERIAL_DRUCKER_PRAGER_END -->   

	<!-- SPHINX_MATERIAL_VISCO_DRUCKER_PRAGER -->
    <ViscoDruckerPrager 
      name="ViscoDruckerPrager"
      defaultDensity="2700"
      defaultBulkModulus="10.0e9"
      defaultShearModulus="6.0e9"
      defaultCohesion="0.1e6"
      defaultFrictionAngle="6.0"
      defaultDilationAngle="3.0"
      defaultHardeningRate="0.5e9"
	  relaxationTime="0.1"
    />
	<!-- SPHINX_MATERIAL_VISCO_DRUCKER_PRAGER_END -->

	<!-- SPHINX_MATERIAL_EXTENDED_DRUCKER_PRAGER -->
	<ExtendedDruckerPrager 
      name="ExtendedDruckerPrager"
      defaultDensity="2700"
      defaultBulkModulus="10.0e9"
      defaultShearModulus="6.0e9"
      defaultCohesion="0.1e6"
      defaultInitialFrictionAngle="6.0"
      defaultResidualFrictionAngle="10.0"
      defaultDilationRatio="0.5"
      defaultHardening="0.0001"
    />  
	<!-- SPHINX_MATERIAL_EXTENDED_DRUCKER_PRAGER_END -->

	<ViscoExtendedDruckerPrager 
      name="ViscoExtendedDruckerPrager"
      defaultDensity="2700"
      defaultBulkModulus="10.0e9"
      defaultShearModulus="6.0e9"
      defaultCohesion="6.0e6"
      defaultInitialFrictionAngle="16.0"
      defaultResidualFrictionAngle="20.0"
      defaultDilationRatio="1.0"
      defaultHardening="0.0001"
	  relaxationTime="0.05"
    />  
  </Constitutive>
  <!-- SPHINX_MATERIAL_END -->

  <!-- SPHINX_FUNCTION -->
  <Functions>
    <TableFunction
      name="strainFunction"
      inputVarNames="{ time }"
      coordinateFiles="{ tables/time.geos }"
      voxelFile="tables/axialStrain.geos"/>

    <TableFunction
      name="stressFunction"
      inputVarNames="{ time }"
      coordinateFiles="{ tables/time.geos }"
      voxelFile="tables/radialStress.geos"/>
  </Functions>
  <!-- SPHINX_FUNCTION_END -->

  <!-- SPHINX_MESH -->
  <Mesh>
    <InternalMesh
      name="mesh1"
      elementTypes="{ C3D8 }"
      xCoords="{ 0, 1 }"
      yCoords="{ 0, 1 }"
      zCoords="{ 0, 1 }"
      nx="{ 1 }"
      ny="{ 1 }"
      nz="{ 1 }"
      cellBlockNames="{ cellBlock01 }"/>
  </Mesh>

  <ElementRegions>
    <CellElementRegion
      name="dummy"
      cellBlocks="{ cellBlock01 }"
      materialList="{ dummy }"/>
  </ElementRegions>
  <!-- SPHINX_MESH_END -->
</Problem>
