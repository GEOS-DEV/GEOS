<?xml version="1.0" ?>
<Problem>
  <Included>
    <File
      name="./SpringSlider_base.xml"/>
  </Included>

  <Events
    maxTime="2.0e5">
    <SoloEvent 
      name="generateFault"
      target="/Solvers/SurfaceGen"/>
      
