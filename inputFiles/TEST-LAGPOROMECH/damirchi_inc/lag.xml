<?xml version="1.0" ?>

<Problem>
  <Solvers
    gravityVector="{ 0.0, 0.0, 0.0 }">
    <SinglePhasePoromechanicsLagrangianContact
      name="porolagrangiancontact"
      contactSolverName="lagrangiancontact"
      contactFlowSolverName="lagrangiancontactflow"
      solidSolverName="lagsolve"
      flowSolverName="SinglePhaseFlow"
      logLevel="1"
      initialDt="8.64e4"
      targetRegions="{ Domain, Fracture }">
      <NonlinearSolverParameters
        newtonTol="1.0e-04"
        timeStepIncreaseIterLimit="0.1"
        timeStepDecreaseIterLimit="0.7"
        logLevel="1"
        newtonMaxIter="10"
        lineSearchAction="Attempt"
        lineSearchMaxCuts="3"
        lineSearchInterpolationType="Parabolic"
        maxTimeStepCuts="2"
        allowNonConverged="0"
        maxNumConfigurationAttempts="10"/>
      <LinearSolverParameters
        directParallel="0"/>
    </SinglePhasePoromechanicsLagrangianContact>

    <LagrangianContactWithFlow 
      name="lagrangiancontactflow"
      fluidSolverName="SinglePhaseFlow"
      contactSolverName="lagrangiancontact"
      stabilizationName="singlePhaseTPFA_stab"
      logLevel="1"
      targetRegions="{Domain, Fracture}"/>

    <LagrangianContact
      name="lagrangiancontact"
      solidSolverName="lagsolve"
      stabilizationName="singlePhaseTPFA_stab"
      logLevel="1"
      targetRegions="{ Domain, Fracture }"
      fractureRegionName="Fracture"
      discretization="FE1"
      contactRelationName="fractureMaterial"/>

    <SolidMechanicsLagrangianSSLE
      name="lagsolve"
      timeIntegrationOption="QuasiStatic"
      discretization="FE1"
      targetRegions="{ Domain }">
      <LinearSolverParameters 
        preconditionerType="amg" 
        amgNullSpaceType="rigidBodyModes"/>  
    </SolidMechanicsLagrangianSSLE>

    <SurfaceGenerator
      name="SurfaceGen"
      targetRegions="{ Domain }"
      rockToughness="1.0e6"
      discretization="FE1"
      mpiCommOrder="1"/>

    <SinglePhaseFVM
      name="SinglePhaseFlow"
      logLevel="1"
      discretization="singlePhaseTPFA"
      targetRegions="{ Domain, Fracture }">
      <LinearSolverParameters
        preconditionerType="direct"
        amgNullSpaceType="constantModes"/>
    </SinglePhaseFVM>
  </Solvers>

  <Mesh>
    <PAMELAMesh
      name="mesh1"
      file="mesh/bar_inclFrac.msh"/>
  </Mesh>

  <Events
    maxTime="8.64e06">

    <SoloEvent
      name="preFracture"
      target="/Solvers/SurfaceGen"/>

    <PeriodicEvent
      name="outputs"
      timeFrequency="8.64e4"
      target="/Outputs/siloOutput"/>

    <PeriodicEvent
      name="solverApplication"
      beginTime="0.0"
      endTime="8.64e06"
      target="/Solvers/porolagrangiancontact"/>
  </Events>

  <NumericalMethods>
    <FiniteElements>
      <FiniteElementSpace
        name="FE1"
        order="1"/>
    </FiniteElements>

    <FiniteVolume>
      <TwoPointFluxApproximation
        name="singlePhaseTPFA"/>

      <TwoPointFluxApproximation
        name="singlePhaseTPFA_stab"/>
    </FiniteVolume>
  </NumericalMethods>

  <ElementRegions>
    <CellElementRegion
      name="Domain"
      cellBlocks="{ DOMAIN_TETRA }"
      materialList="{ porousRock, fluid }"/>

    <SurfaceElementRegion
      name="Fracture"
      subRegionType="faceElement"
      defaultAperture="0.005"
      materialList="{ fluid, fractureFilling, fractureMaterial }"/>
  </ElementRegions>

  <Constitutive>
    <PorousElasticIsotropic
      name="porousRock"
      solidModelName="skeleton"
      porosityModelName="skeletonPorosity"
      permeabilityModelName="skeletonPerm"/>

    <ElasticIsotropic
      name="skeleton"
      defaultDensity="0"
      defaultYoungModulus="40.0e6"
      defaultPoissonRatio="0.3"/>

    <CompressibleSinglePhaseFluid
      name="fluid"
      defaultDensity="1.0e03"
      defaultViscosity="1.0e-03"
      referencePressure="0.0"
      referenceDensity="1.0e03"
      compressibility="5.0e-9"
      referenceViscosity="1.0e-03"
      viscosibility="0.0"/>

    <BiotPorosity
      name="skeletonPorosity"
      grainBulkModulus="1.0e27"
      defaultReferencePorosity="0.1"/>

    <ConstantPermeability
      name="skeletonPerm"
      permeabilityComponents="{ 1.0e-15, 1.0e-15, 1.0e-15 }"/>

    <Coulomb
      name="fractureMaterial"
      cohesion="0.0"
      frictionCoefficient="0.6"
      apertureTableName="aperTable"/>

    <CompressibleSolidConstantPlusParallelPlatesPermeability 
      name="fractureFilling"
      solidModelName="nullSolid"
      porosityModelName="fracturePorosity"
      permeabilityModelName="fracturePerm"/>

    <!--CompressibleSolidConstantPermeability 
      name="fractureFilling"
      solidModelName="nullSolid"
      porosityModelName="fracturePorosity"
      permeabilityModelName="fracturePerm"/-->

    <NullModel 
      name="nullSolid"/>

    <PressurePorosity 
      name="fracturePorosity"
      defaultReferencePorosity="0.05"
      referencePressure="0.0"
      compressibility="0.0"/>

    <ConstantPlusParallelPlatesPermeability
      name="fracturePerm"
      defaultConductivity="9.869233e-11"/>

    <!--ConstantPermeability
      name="fracturePerm"
      permeabilityComponents="{1e-10, 1e-10, 1e-10}"/-->
  </Constitutive>

  <FieldSpecifications>
	
    <!-- condition on the fracture at time t0 -->
    <FieldSpecification
      name="frac"
      initialCondition="1"
      setNames="{ FRACTURE_0 }"
      objectPath="faceManager"
      fieldName="ruptureState"
      scale="1"/>

    <FieldSpecification
      name="separableFace"
      initialCondition="1"
      setNames="{ FRACTURE_0 }"
      objectPath="faceManager"
      fieldName="isFaceSeparable"
      scale="1"/>

  <!-- condition on the pressure at time t0 -->
    <FieldSpecification
      name="initialPressure"
      initialCondition="1"
      setNames="{ all }"
      objectPath="ElementRegions/Domain/DOMAIN_TETRA"
      fieldName="pressure"
      scale="0.0"/>

  <!-- condition on the displacements at time t0 -->
    <FieldSpecification
      name="xInitialDisplacement"
      initialCondition="1"
      setNames="{ all }"
      objectPath="nodeManager"
      fieldName="TotalDisplacement"
      component="0"
      scale="0.0"/>

    <FieldSpecification
      name="yInitialDisplacement"
      initialCondition="1"
      setNames="{ all }"
      objectPath="nodeManager"
      fieldName="TotalDisplacement"
      component="1"
      scale="0.0"/>

    <FieldSpecification
      name="zInitialDisplacement"
      initialCondition="1"
      setNames="{ all }"
      objectPath="nodeManager"
      fieldName="TotalDisplacement"
      component="2"
      scale="0.0"/>

  <!-- boundary condition on displacements -->
    <FieldSpecification
      name="xconstraint"
      objectPath="nodeManager"
      fieldName="TotalDisplacement"
      component="0"
      scale="0.0"
      setNames="{ XMIN, XMAX, ZMIN }"/>

    <FieldSpecification
      name="yconstraint"
      objectPath="nodeManager"
      fieldName="TotalDisplacement"
      component="1"
      scale="0.0"
      setNames="{ YMIN, YMAX }"/>

    <FieldSpecification
      name="zconstraint"
      objectPath="nodeManager"
      fieldName="TotalDisplacement"
      component="2"
      scale="0.0"
      setNames="{ ZMIN }"/>

  <!-- imposed traction on the top -->
    <Traction
      name="zposconstraint"
      objectPath="faceManager"
      direction="{ 0, 0, -1 }"
      scale="1.0e4"
      setNames="{ ZMAX }"
      functionName="timeFunction"/>

  <!-- imposed pressure on the top -->
    <FieldSpecification
      name="boundaryPressure"
      setNames="{ ZMAX }"
      objectPath="faceManager"
      fieldName="pressure"
      scale="0.0"/>

  </FieldSpecifications>

  <Functions>
    <TableFunction
      name="timeFunction"
      inputVarNames="{ time }"
      coordinates="{ 0.0, 0.1e-09, 1e7 }"
      values="{ 0.0, 1.0, 1.0 }"/>

    <TableFunction
      name="aperTable"
      coordinates="{ -1.0e-3, 0.0 }"
      values="{ 0.e-3, 0.e-3 }"/>
  </Functions>

  <Outputs>
    <Silo
      name="siloOutput"/>
  </Outputs>

</Problem>
