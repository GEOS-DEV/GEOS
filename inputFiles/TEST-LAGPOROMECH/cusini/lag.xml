<?xml version="1.0" ?>

<Problem>
  <Solvers
    gravityVector="{ 0.0, 0.0, 0.0 }">
    <SinglePhasePoromechanicsLagrangianContact
      name="porolagrangiancontact"
      contactSolverName="lagrangiancontact"
      contactFlowSolverName="lagrangiancontactflow"
      solidSolverName="lagsolve"
      fluidSolverName="SinglePhaseFlow"
      logLevel="1"
      initialDt="0.1"
      discretization="FE1"
      targetRegions="{ Domain, Fracture }">
      <NonlinearSolverParameters
        newtonTol="1.0e-04"
        dtIncIterLimit="0.1"
        dtCutIterLimit="0.7"
        logLevel="1"
        newtonMaxIter="10"
        lineSearchAction="Attempt"
        lineSearchMaxCuts="3"
        lineSearchInterpolationType="Parabolic"
        maxTimeStepCuts="2"
        allowNonConverged="0"
        maxNumConfigurationAttempts="10"/>
      <LinearSolverParameters
        directParallel="0"/>
    </SinglePhasePoromechanicsLagrangianContact>

    <LagrangianContactWithFlow 
      name="lagrangiancontactflow"
      fluidSolverName="SinglePhaseFlow"
      contactSolverName="lagrangiancontact"
      stabilizationName="singlePhaseTPFA_stab"
      logLevel="1"
      targetRegions="{Domain, Fracture}"/>

    <LagrangianContact
      name="lagrangiancontact"
      solidSolverName="lagsolve"
      stabilizationName="singlePhaseTPFA_stab"
      logLevel="1"
      targetRegions="{ Domain, Fracture }"
      contactRelationName="fractureMaterial"/>

    <SolidMechanicsLagrangianSSLE
      name="lagsolve"
      timeIntegrationOption="QuasiStatic"
      discretization="FE1"
      targetRegions="{ Domain }">
      <LinearSolverParameters
        preconditionerType="amg"
        amgNullSpaceType="rigidBodyModes"/>
    </SolidMechanicsLagrangianSSLE>

    <SurfaceGenerator
      name="SurfaceGen"
      targetRegions="{ Domain }"
      rockToughness="1.0e6"
      mpiCommOrder="1"/>

    <SinglePhaseFVM
      name="SinglePhaseFlow"
      logLevel="1"
      discretization="singlePhaseTPFA"
      targetRegions="{ Domain, Fracture }">
      <LinearSolverParameters
        preconditionerType="direct"
        amgNullSpaceType="constantModes"/>
    </SinglePhaseFVM>
  </Solvers>

  <Mesh>
    <PAMELAMesh
      name="mesh1"
      file="mesh/inclFracRes.msh"/>
  </Mesh>

  <Events
    maxTime="10.0">

    <SoloEvent
      name="preFracture"
      target="/Solvers/SurfaceGen"/>

    <PeriodicEvent
      name="outputs"
      timeFrequency="0.1"
      target="/Outputs/siloOutput"/>

    <PeriodicEvent
      name="solverApplication"
      beginTime="0.0"
      endTime="10.0"
      target="/Solvers/porolagrangiancontact"/>
  </Events>

  <NumericalMethods>
    <FiniteElements>
      <FiniteElementSpace
        name="FE1"
        order="1"/>
    </FiniteElements>

    <FiniteVolume>
      <TwoPointFluxApproximation
        name="singlePhaseTPFA"/>

      <TwoPointFluxApproximation
        name="singlePhaseTPFA_stab"/>
    </FiniteVolume>
  </NumericalMethods>

  <ElementRegions>
    <CellElementRegion
      name="Domain"
      cellBlocks="{ DOMAIN_HEX }"
      materialList="{ porousRock, fluid }"/>

    <SurfaceElementRegion
      name="Fracture"
      subRegionType="faceElement"
      defaultAperture="0.0"
      materialList="{ fluid, fractureFilling, fractureMaterial }"/>
  </ElementRegions>

  <Constitutive>
    <PorousElasticIsotropic
      name="porousRock"
      solidModelName="skeleton"
      porosityModelName="skeletonPorosity"
      permeabilityModelName="skeletonPerm"/>

    <ElasticIsotropic
      name="skeleton"
      defaultDensity="0"
      defaultYoungModulus="9.0e9"
      defaultPoissonRatio="0.4"/>

    <CompressibleSinglePhaseFluid
      name="fluid"
      defaultDensity="1.0e03"
      defaultViscosity="1.0e-03"
      referencePressure="0.0"
      referenceDensity="1.0e03"
      compressibility="0.0e0"
      referenceViscosity="1.0e-03"
      viscosibility="0.0"/>

    <BiotPorosity
      name="skeletonPorosity"
      grainBulkModulus="1.0e27"
      defaultReferencePorosity="0.3"/>

    <ConstantPermeability
      name="skeletonPerm"
      permeabilityComponents="{ 1.0e-12, 1.0e-12, 1.0e-12 }"/>

    <Coulomb
      name="fractureMaterial"
      cohesion="0.0"
      frictionCoefficient="0.6"
      apertureTableName="aperTable"/>

    <CompressibleSolidConstantPlusParallelPlatesPermeability 
      name="fractureFilling"
      solidModelName="nullSolid"
      porosityModelName="fracturePorosity"
      permeabilityModelName="fracturePerm"/>

    <CompressibleSolidConstantPermeability 
      name="fractureFilling"
      solidModelName="nullSolid"
      porosityModelName="fracturePorosity"
      permeabilityModelName="fracturePerm"/>

    <NullModel 
      name="nullSolid"/>

    <PressurePorosity 
      name="fracturePorosity"
      defaultReferencePorosity="1.00"
      referencePressure="0.0"
      compressibility="0.0"/>

    <ConstantPlusParallelPlatesPermeability
      name="fracturePerm"
      defaultConductivity="9.869233e-05"/>
  </Constitutive>

  <FieldSpecifications>
	
    <!-- condition on the fracture at time t0 -->
    <FieldSpecification
      name="frac"
      initialCondition="1"
      setNames="{ FRACTURE }"
      objectPath="faceManager"
      fieldName="ruptureState"
      scale="1"/>

    <FieldSpecification
      name="separableFace"
      initialCondition="1"
      setNames="{ FRACTURE }"
      objectPath="faceManager"
      fieldName="isFaceSeparable"
      scale="1"/>

  <!-- condition on the pressure at time t0 -->
    <FieldSpecification
      name="initialPressure"
      initialCondition="1"
      setNames="{ all }"
      objectPath="ElementRegions/Domain/DOMAIN_HEX"
      fieldName="pressure"
      scale="0.0"/>

  <!-- condition on the displacements at time t0 -->
    <FieldSpecification
      name="xInitialDisplacement"
      initialCondition="1"
      setNames="{ all }"
      objectPath="nodeManager"
      fieldName="TotalDisplacement"
      component="0"
      scale="0.0"/>

    <FieldSpecification
      name="yInitialDisplacement"
      initialCondition="1"
      setNames="{ all }"
      objectPath="nodeManager"
      fieldName="TotalDisplacement"
      component="1"
      scale="0.0"/>

    <FieldSpecification
      name="zInitialDisplacement"
      initialCondition="1"
      setNames="{ all }"
      objectPath="nodeManager"
      fieldName="TotalDisplacement"
      component="2"
      scale="0.0"/>

  <!-- boundary condition on displacements -->
    <FieldSpecification
      name="xconstraint"
      objectPath="nodeManager"
      fieldName="TotalDisplacement"
      component="0"
      scale="0.0"
      setNames="{ XMIN, XMAX }"/>

    <FieldSpecification
      name="yconstraint"
      objectPath="nodeManager"
      fieldName="TotalDisplacement"
      component="1"
      scale="0.0"
      setNames="{ YMIN }"/>

    <FieldSpecification
      name="zconstraint"
      objectPath="nodeManager"
      fieldName="TotalDisplacement"
      component="2"
      scale="0.0"
      setNames="{ ZMIN, ZMAX }"/>

  <!-- imposed pressure on the top -->
    <FieldSpecification
      name="boundaryPressure"
      setNames="{ YMAX }"
      objectPath="faceManager"
      fieldName="pressure"
      scale="0.0"/>

    <Aquifer
      name="aquiferBC"
      aquiferPorosity="1.0"
      aquiferPermeability="1.0"
      aquiferInitialPressure="1.0e0"
      aquiferWaterViscosity="1.0"
      aquiferWaterDensity="1.0"
      aquiferTotalCompressibility="1.0"
      aquiferElevation="1"
      aquiferThickness="1"
      aquiferInnerRadius="1"
      aquiferAngle="90"
      setNames="{ YMIN }"/>

  </FieldSpecifications>

  <Functions>
    <TableFunction
      name="aperTable"
      coordinates="{ -1.0e-3, 0.0 }"
      values="{ 5.e-3, 5.e-3 }"/>
  </Functions>

  <Outputs>
    <Silo
      name="siloOutput"/>
  </Outputs>

</Problem>
