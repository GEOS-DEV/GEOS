<?xml version="1.0" ?>

<Problem>
  <Included>
    <File name="./ThermoPoroElastic_conforming_base.xml" />
  </Included>
  <Outputs>
    <VTK
      name="vtkOutput"
      plotFileRoot="ThermoPoroElastic_conforming_verticalFrac"
      plotLevel="2"
      format="binary"/>
    <TimeHistory
      name="pressureHistoryOutput"
      sources="{/Tasks/pressureCollection}"
      filename="pressure_history"/> 
    <TimeHistory
      name="temperatureHistoryOutput"
      sources="{/Tasks/temperatureCollection}"
      filename="temperature_history"/> 
    <TimeHistory
      name="apertureOutput"
      sources="{/Tasks/apertureCollection}"
      filename="aperture_history"/> 
    <Restart
      name="restartOutput"/>
  </Outputs>

  <Tasks>
    <PackCollection
      name="pressureCollection"
      objectPath="ElementRegions/RockMatrix/cb1"
      fieldName="pressure"/>

    <PackCollection
      name="apertureCollection"
      objectPath="ElementRegions/Fracture/FractureSubRegion"
      fieldName="hydraulicAperture"/>

    <PackCollection
      name="temperatureCollection"
      objectPath="ElementRegions/RockMatrix/cb1"
      fieldName="temperature"/>           
  </Tasks>
  

</Problem>
