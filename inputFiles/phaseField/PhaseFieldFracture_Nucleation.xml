<?xml version="1.0" ?>

<Problem>
  <Solvers
    gravityVector="{ 0.0, 0.0, 0.0 }">
    <PhaseFieldFracture
      name="PhaseFieldSolve"
      solidSolverName="SolidMechSolve"
      damageSolverName="DamageSolve"
      couplingTypeOption="FixedStress"
      logLevel="2"
      subcycling="0"
      discretization="FE1"
      targetRegions="{ Region1 }">
      <NonlinearSolverParameters
        newtonMaxIter="100"/>
      <LinearSolverParameters
        directParallel="0"/>
    </PhaseFieldFracture>

    <SolidMechanicsLagrangianSSLE
      name="SolidMechSolve"
      timeIntegrationOption="QuasiStatic"
      logLevel="1"
      discretization="FE1"
      targetRegions="{ Region1 }"
      >
      <NonlinearSolverParameters
        newtonMaxIter="200"
        newtonTol="1.0e-12"/>
      <LinearSolverParameters
        directParallel="0"/>
    </SolidMechanicsLagrangianSSLE>

    <PhaseFieldDamageFEM
      name="DamageSolve"
      discretization="FE1"
      timeIntegrationOption="SteadyState"
      fieldName="Damage"
      
      localDissipation="Linear"
      logLevel="1"
      targetRegions="{ Region1 }">
      <NonlinearSolverParameters
        newtonMaxIter="200"
        newtonTol="1.0e-12"/>
      <LinearSolverParameters
        directParallel="0"/>
    </PhaseFieldDamageFEM>

    <SurfaceGenerator
      name="SurfaceGen"
      targetRegions="{ Region1 }"
      nodeBasedSIF="1"
      
      rockToughness="0"
      mpiCommOrder="1"/>
  </Solvers>

  <Mesh>
    <InternalMesh
      name="mesh1"
      elementTypes="{ C3D8 }"
      xCoords="{ 0, 30 }"
      yCoords="{ -5, -1.0, 0, 1.0, 5 }"
      zCoords="{ 0, 1 }"
      nx="{ 120 }"
      ny="{ 20, 20, 20, 20 }"
      nz="{ 2 }"
      yBias="{0.75, 0, 0, -0.75}"
      cellBlockNames="{ cb1 }"/>
  </Mesh>

  <Events
    maxTime="1.0">
    <!-- <SoloEvent name="siloOutput0"
               target="/Outputs/SiloOutput"/> -->
    <SoloEvent
      name="preFracture"
      target="/Solvers/SurfaceGen"/>
    <!-- This event is applied every cycle, and overrides the
    solver time-step request -->
    <PeriodicEvent
      name="solverApplications"
      forceDt="0.01"
      target="/Solvers/PhaseFieldSolve"/>

    <!-- This event is applied every 1.0s.  The targetExactTimestep
    flag allows this event to request a dt modification to match an
    integer multiple of the timeFrequency. -->
    <PeriodicEvent
      name="vtkOutput"
      timeFrequency="0.01"
      target="/Outputs/DamageNucleation_VTKOutput"/>

    <PeriodicEvent
      name="siloOutputs"
      timeFrequency="0.01"
      target="/Outputs/DamageNucleation_SiloOutput"/>

    <PeriodicEvent
      name="restarts"
      timeFrequency="0.5"
      targetExactTimestep="0"
      target="/Outputs/restartOutput"/>
  </Events>

  <NumericalMethods>
    <FiniteElements>
      <FiniteElementSpace
        name="FE1"
        order="1"/>
    </FiniteElements>
  </NumericalMethods>

  <ElementRegions>
    <CellElementRegion
      name="Region1"
      cellBlocks="{ cb1 }"
      materialList="{ graphite }"/>

    <SurfaceElementRegion
      name="Fracture"
      defaultAperture="0.0"
      materialList="{ graphite }"/>
  </ElementRegions>

  <Constitutive>
    <DamageElasticIsotropic
      name="graphite"
      defaultDensity="2700"
      defaultBulkModulus="4.4144e3"
      defaultShearModulus="4.3363e3"
      lengthScale="0.2"
      criticalFractureEnergy="9.1e-2"
      criticalStrainEnergy="0"
      extDrivingForceSwitch="True"
      tensileStrength="27.0"
      compressiveStrength="77.0"
      deltaCoefficient="3.22"/>
  </Constitutive>

  <FieldSpecifications>
    <FieldSpecification
      name="yPosDisp"
      fieldName="TotalDisplacement"
      component="1"
      objectPath="nodeManager"
      scale="1.3737e-3"
      functionName="disp"
      setNames="{ ypos }"/>

    <FieldSpecification
      name="yNegDisp"
      fieldName="TotalDisplacement"
      component="1"
      objectPath="nodeManager"
      scale="-1.3737e-3"
      functionName="disp"
      setNames="{ yneg }"/>

    <FieldSpecification
      name="fixedY"
      fieldName="TotalDisplacement"
      component="1"
      objectPath="nodeManager"
      scale="0.0"
      setNames="{ rightCenterPointy }"/>

    <FieldSpecification
      name="fixedX"
      fieldName="TotalDisplacement"
      component="0"
      objectPath="nodeManager"
      scale="0.0"
      setNames="{ xpos }"/>

    <FieldSpecification
      name="fixedZ"
      fieldName="TotalDisplacement"
      component="2"
      objectPath="nodeManager"
      scale="0.0"
      setNames="{ rightCenterPointz }"/>

    <FieldSpecification
      name="separableFace"
      initialCondition="1"
      setNames="{ fracture }"
      objectPath="faceManager"
      fieldName="isFaceSeparable"
      scale="1"/>

    <FieldSpecification
      name="frac"
      initialCondition="1"
      setNames="{ fracture }"
      objectPath="faceManager"
      fieldName="ruptureState"
      scale="1"/>
  </FieldSpecifications>

  <Functions>
    <TableFunction
      name="disp"
      inputVarNames="{ ReferencePosition, time }"
      coordinateFiles="{ surfing_bc_tables/xlin.geos, surfing_bc_tables/ylin.geos, surfing_bc_tables/zlin.geos, surfing_bc_tables/tlin.geos }"
      voxelFile="surfing_bc_tables/disp.geos"/>
  </Functions>

  <Outputs>
    <Silo
      name="DamageNucleation_SiloOutput"/>

    <VTK
      name="DamageNucleation_VTKOutput"
      plotFileRoot="plot_damage_edf_off"/>

    <Restart
      name="restartOutput"/>
  </Outputs>

  <Geometry>
    <Box
      name="fracture"
      xMin="{ -0.001, -0.001, -0.001 }"
      xMax="{  5.001,  0.001,  1.001 }"/>

    <Box
      name="rightCenterPointy"
      xMin="{ 29.999, -0.001, -0.001 }"
      xMax="{ 30.001, 0.001, 1.001 }"/>

    <Box
      name="rightCenterPointz"
      xMin="{ 29.999, -5.001, 0.499 }"
      xMax="{ 30.001, 5.001, 0.501 }"/>
  </Geometry>
</Problem>
