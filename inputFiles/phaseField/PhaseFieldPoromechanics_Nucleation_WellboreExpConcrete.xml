<?xml version="1.0" ?>

<Problem>
  <Solvers
    gravityVector="{ 0.0, 0.0, 0.0 }">
    <PhaseFieldPoromechanics
      name="phasefieldPoroSolve"
      poromechanicsSolverName="poroSolve"
      damageSolverName="damageSolve"
      logLevel="1"
      targetRegions="{ Region1 }">
    </PhaseFieldPoromechanics>

    <SinglePhasePoromechanics
      name="poroSolve"
      solidSolverName="solidMech"
      flowSolverName="singlePhaseFlow"
      logLevel="1"
      targetRegions="{ Region1 }"
      damageFlag="1">
      <NonlinearSolverParameters
        newtonMaxIter="100"
        newtonTol="1.0e-6"/>
      <LinearSolverParameters
        solverType="gmres"
        preconditionerType="mgr"
        krylovTol="1e-4"/>
    </SinglePhasePoromechanics>

    <PhaseFieldDamageFEM
      name="damageSolve"
      discretization="FE1"
      timeIntegrationOption="SteadyState"
      fieldName="Damage"
      localDissipation="Linear"
      irreversibilityFlag="1"
      damageUpperBound="1.0"
      fracturePressureTermFlag="1"
      logLevel="1"
      targetRegions="{ Region1 }">
      <NonlinearSolverParameters
        newtonMaxIter="200"
        newtonTol="1.0e-4"/>
      <LinearSolverParameters
        solverType="gmres"
        preconditionerType="amg"/>
    </PhaseFieldDamageFEM>

    <SolidMechanicsLagrangianSSLE
      name="solidMech"
      timeIntegrationOption="QuasiStatic"
      logLevel="1"
      discretization="FE1"
      targetRegions="{ Region1 }"/>

    <SinglePhaseHybridFVM
      name="singlePhaseFlow"
      logLevel="1"
      discretization="singlePhaseHybridMimetic"
      targetRegions="{ Region1 }"/>
  </Solvers>

  <Mesh>
    <VTKMesh 
      name="mesh"
      regionAttribute="MIDDLE_RING-LOWER_RING-UPPER_RING-MATRIX"
      file="WellboreMesh/wellboreFine.vtu"
      nodesetNames="{ xneg, xpos, yneg, ypos, zneg, zpos, hole_middle, hole_upper, hole_lower }"/>
  </Mesh>

  <Events
    maxTime="1017.0">
 
    <PeriodicEvent
      name="solverApplication0"
      endTime="1000.0"
      forceDt="1000.0"
      target="/Solvers/phasefieldPoroSolve"/>

    <PeriodicEvent
      name="solverApplication1"
      beginTime="1000.0"
      endTime="1015.5"
      forceDt="0.5"
      target="/Solvers/phasefieldPoroSolve"/>

    <PeriodicEvent
      name="solverApplication2"
      beginTime="1015.5"
      endTime="1017.0"
      forceDt="0.02"
      target="/Solvers/phasefieldPoroSolve"/>

    <PeriodicEvent
      name="vtkOutput0"
      endTime="1000.0"
      forceDt="1000.0"
      target="/Outputs/VTKOutput"/>

    <PeriodicEvent
      name="vtkOutput1"
      beginTime="1000.0"
      endTime="1015.5"
      forceDt="0.5"
      target="/Outputs/VTKOutput"/>
    
    <PeriodicEvent
      name="vtkOutput2"
      beginTime="1015.5"
      endTime="1017.0"
      forceDt="0.02"
      target="/Outputs/VTKOutput"/>

    <PeriodicEvent
      name="restarts"
      timeFrequency="1000"
      targetExactTimestep="0"
      target="/Outputs/restartOutput"/>
  </Events>

  <NumericalMethods>
    <FiniteElements>
      <FiniteElementSpace 
        name="FE1"
        order="1"/>
    </FiniteElements>
    <FiniteVolume>
      <HybridMimeticDiscretization
        name="singlePhaseHybridMimetic"
        innerProductType="quasiTPFA"/>
    </FiniteVolume>
  </NumericalMethods>

  <ElementRegions>
    <CellElementRegion
      name="Region1"
      cellBlocks="{ 0_hexahedra, 1_hexahedra, 2_hexahedra, 3_hexahedra }"
      materialList="{ concrete, water }"/>
  </ElementRegions>

  <Constitutive>
    <PorousDamageElasticIsotropic
      name="concrete"
      solidModelName="concreteSolid"
      porosityModelName="concretePorosity"
      permeabilityModelName="concretePerm"/>

    <DamageElasticIsotropic
      name="concreteSolid"
      defaultDensity="2.7e-9"
      defaultBulkModulus="1.67e4"
      defaultShearModulus="1.25e4"
      lengthScale="1.0"
      defaultCriticalFractureEnergy="4e-3"
      criticalStrainEnergy="0"
      extDrivingForceFlag="1"
      degradationLowerLimit="1e-6"
      defaultTensileStrength="5.5"
      compressiveStrength="40.0"
      deltaCoefficient="3.28"/>

    <BiotPorosity
      name="concretePorosity"
      grainBulkModulus="8.35e4"
      defaultReferencePorosity="0.1"/>

    <DamagePermeability
      name="concretePerm"
      bulkPermeability="1.0e-9"/>

    <CompressibleSinglePhaseFluid
      name="water"
      defaultDensity="1e-9"
      defaultViscosity="1.0e-9"
      referencePressure="0.000"
      referenceDensity="1e-9"
      compressibility="0.0"
      referenceViscosity="1.0e-9"
      viscosibility="0.0"/>
  </Constitutive>

  <FieldSpecifications>    
    <FieldSpecification
       name="Gc"
       initialCondition="1"
       setNames="{ all }"
       objectPath="ElementRegions/Region1"
       fieldName="concreteSolid_criticalFractureEnergy"
       scale="4e-3"/>

    <FieldSpecification
      name="tensileStrength"
      initialCondition="1"
      setNames="{ all }"
      objectPath="ElementRegions/Region1"
      fieldName="concreteSolid_tensileStrength"
      scale="5.5"/>
    
    <FieldSpecification 
      name="Sh"
      initialCondition="1"
      setNames="{ all }"
      objectPath="ElementRegions/Region1"
      fieldName="concreteSolid_stress"
      component="0"
      scale="-10.0"/>

    <FieldSpecification 
      name="SH"
      initialCondition="1"
      setNames="{ all }"
      objectPath="ElementRegions/Region1"
      fieldName="concreteSolid_stress"
      component="1"
      scale="-15.0"/>

    <FieldSpecification 
      name="Sv"
      initialCondition="1"
      setNames="{ all }"
      objectPath="ElementRegions/Region1"
      fieldName="concreteSolid_stress"
      component="2"
      scale="-17.5"/>

    <FieldSpecification 
      name="xconstraint"    
      objectPath="nodeManager" 
      fieldName="totalDisplacement" 
      component="0" 
      scale="0.0" 
      setNames="{xneg, xpos}"/>

    <FieldSpecification 
      name="yconstraint"    
      objectPath="nodeManager" 
      fieldName="totalDisplacement" 
      component="1" 
      scale="0.0" 
      setNames="{yneg, ypos}"/>

    <FieldSpecification 
      name="zconstraint" 
      objectPath="nodeManager" 
      fieldName="totalDisplacement" 
      component="2" 
      scale="0.0" 
      setNames="{zneg, zpos}"
    />  

    <FieldSpecification 
      name="initialPressure"
      initialCondition="1"
      setNames="{all}"
      objectPath="ElementRegions/Region1"
      fieldName="pressure"
      scale="0.0"
    />

    <FieldSpecification 
      name="pressureConstraint"
      setNames="{xminus, xplus, yminus, yplus}"
      objectPath="ElementRegions/Region1"
      fieldName="pressure"
      scale="0.0"
    />

    <FieldSpecification
      name="sourceTerm"
      objectPath="ElementRegions/Region1/0_hexahedra"
      fieldName="pressure"
      scale="100.0"
      setNames="{ all }"
      functionName="timeFunction"
      />

    <FieldSpecification
      name="pressureUpper"
      objectPath="ElementRegions/Region1/1_hexahedra"
      fieldName="pressure"
      scale="0.0"
      setNames="{ all }"
      functionName="timeFunction"
      />
    
    <FieldSpecification
      name="pressureLower"
      objectPath="ElementRegions/Region1/2_hexahedra"
      fieldName="pressure"
      scale="0.0"
      setNames="{ all }"
      functionName="timeFunction"
      />

    <Traction 
      name="traction"
      setNames="{ hole_middle }"
      objectPath="faceManager"
      scale="-100.0"
      tractionType="normal"
      nodalScaleFlag="1"
      functionName="timeFunction"    
      />
  </FieldSpecifications>

  <Functions>
    <TableFunction
      name="timeFunction"
      inputVarNames="{ time }"
      coordinates="{ 0, 1e3, 1.1e3 }"
      values="{ 0.0, 0.0, 1.0 }"/>
  </Functions>

  <Geometry>
    <Box
      name="xplus"
      xMin="{ 98.9, -100.1, -0.1 }"
      xMax="{ 100.1, 100.1, 200.1 }"/>

    <Box
      name="xminus"
      xMin="{ -100.1, -100.1, -0.1 }"
      xMax="{ -98.9, 100.1, 200.1 }"/>

    <Box
      name="yplus"
      xMin="{-100.1, 98.9, -0.1 }"
      xMax="{ 100.1, 100.1, 200.1 }"/>

    <Box
      name="yminus"
      xMin="{ -100.1, -100.1, -0.1 }"
      xMax="{ 100.1, -98.9, 200.1 }"/>
  </Geometry>

  <Outputs>
    <VTK
      name="VTKOutput"
      plotFileRoot="phaseFieldPoro_nucleation_wellboreConcreteNormal_b=0p8_l=1_delta=3p28"/>

    <Restart
      name="restartOutput"/>
  </Outputs>

</Problem>
