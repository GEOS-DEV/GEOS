<?xml version="1.0" ?>

<Problem>
  <Solvers
    gravityVector="{ 0.0, 0.0, 0.0 }">
    <PhaseFieldPoromechanics
      name="phasefieldPoroSolve"
      poromechanicsSolverName="poroSolve"
      damageSolverName="damageSolve"
      couplingTypeOption="FixedStress"
      logLevel="1"
      subcycling="0"
      discretization="FE1"
      targetRegions="{ Region1 }">
    </PhaseFieldPoromechanics>

    <SinglePhasePoromechanics
      name="poroSolve"
      solidSolverName="solidMech"
      flowSolverName="singlePhaseFlow"
      logLevel="1"
      discretization="FE1"
      targetRegions="{ Region1 }"
      damageFlag="1">
      <NonlinearSolverParameters
        newtonMaxIter="100"
        newtonTol="1.0e-6"/>
      <LinearSolverParameters
        solverType="direct"
        directParallel="0"
        logLevel="0"/>
    </SinglePhasePoromechanics>

    <PhaseFieldDamageFEM
      name="damageSolve"
      discretization="FE1"
      timeIntegrationOption="SteadyState"
      fieldName="Damage"
      localDissipation="Quadratic"
      logLevel="1"
      targetRegions="{ Region1 }">
      <NonlinearSolverParameters
        newtonMaxIter="200"
        newtonTol="1.0e-6"/>
      <LinearSolverParameters
        directParallel="0"/>
    </PhaseFieldDamageFEM>

    <SolidMechanicsLagrangianSSLE
      name="solidMech"
      timeIntegrationOption="QuasiStatic"
      logLevel="1"
      discretization="FE1"
      targetRegions="{ Region1 }"/>

    <SinglePhaseFVM
      name="singlePhaseFlow"
      logLevel="1"
      discretization="singlePhaseTPFA"
      targetRegions="{ Region1 }"/>
  </Solvers>

  <Mesh>
    <InternalMesh
      name="mesh1"
      elementTypes="{ C3D8 }"
      xCoords="{ 0, 10 }"
      yCoords="{ 0, 10 }"
      zCoords="{ 0, 0.1 }"
      nx="{ 200 }"
      ny="{ 200 }"
      nz="{ 1 }"
      cellBlockNames="{ cb1 }"/>
  </Mesh>

  <Events
    maxTime="1.0">
 
    <PeriodicEvent
      name="solverApplications"
      forceDt="1.0"
      target="/Solvers/phasefieldPoroSolve"/>

    <PeriodicEvent
      name="vtkOutput"
      timeFrequency="0.01"
      target="/Outputs/WellboreVTKOutput"/>

    <PeriodicEvent
      name="restarts"
      timeFrequency="0.5"
      targetExactTimestep="0"
      target="/Outputs/restartOutput"/>
  </Events>

  <NumericalMethods>
    <FiniteElements>
      <FiniteElementSpace 
        name="FE1"
        order="1"/>
    </FiniteElements>
    <FiniteVolume>
      <TwoPointFluxApproximation 
        name="singlePhaseTPFA"/>
    </FiniteVolume>
  </NumericalMethods>

  <ElementRegions>
    <CellElementRegion
      name="Region1"
      cellBlocks="{ cb1 }"
      materialList="{ rock, water }"/>
  </ElementRegions>

  <Constitutive>
    <PorousDamageElasticIsotropic
      name="rock"
      solidModelName="rockSolid"
      porosityModelName="rockPorosity"
      permeabilityModelName="rockPerm"/>

    <DamageElasticIsotropic
      name="rockSolid"
      defaultDensity="2700"
      defaultBulkModulus="5e10"
      defaultShearModulus="2.308e10"
      lengthScale="1e-1"
      criticalFractureEnergy="5e2"
      criticalStrainEnergy="0"
      extDrivingForceSwitch="False"/>

    <BiotPorosity
      name="rockPorosity"
      grainBulkModulus="1.0e28"
      defaultReferencePorosity="0.05"/>

    <DamagePermeability
      name="rockPerm"
      bulkPermeability="1.0e-15"
      fracturePermeability="1.0e-5"/>

    <CompressibleSinglePhaseFluid
      name="water"
      defaultDensity="1000"
      defaultViscosity="1.0e-3"
      referencePressure="0.000"
      referenceDensity="1000"
      compressibility="4.4e-10"
      referenceViscosity="1.0e-9"
      viscosibility="0.0"/>
  </Constitutive>

  <FieldSpecifications>    
    <FieldSpecification 
      name="initialPressure"
      initialCondition="1"
      setNames="{all}"
      objectPath="ElementRegions/Region1/cb1"
      fieldName="pressure"
      scale="0.0"/>

    <FieldSpecification 
      name="initialDamage"
      setNames="{ fracture }"
      objectPath="nodeManager"
      fieldName="Damage"
      scale="1.0"/>

    <FieldSpecification
      name="fixedY"
      fieldName="TotalDisplacement"
      component="1"
      objectPath="nodeManager"
      scale="0.0"
      setNames="{ ypos, yneg }"/>

    <FieldSpecification
      name="fixedX"
      fieldName="TotalDisplacement"
      component="0"
      objectPath="nodeManager"
      scale="0.0"
      setNames="{ xpos, xneg }"/>

    <FieldSpecification
      name="fixedZ"
      fieldName="TotalDisplacement"
      component="2"
      objectPath="nodeManager"
      scale="0.0"
      setNames="{ zpos, zneg }"/> 

    <FieldSpecification 
      name="outerPressure"
      setNames="{ xpos, xneg, ypos, yneg }"
      objectPath="faceManager"
      fieldName="pressure"
      scale="0.0" 
    />

    <SourceFlux
      name="sourceTerm"
      objectPath="ElementRegions/Region1/cb1"
      scale="-0.01"
      component="0"
      setNames="{ source }"
      functionName="timeFunction"/>
  </FieldSpecifications>

  <Functions>
    <TableFunction
      name="timeFunction"
      inputVarNames="{ time }"
      coordinates="{ 0, 0.02, 1 }"
      values="{ 0.0, 0.0, 1.0 }"/>
  </Functions>

  <Outputs>
    <VTK
      name="WellboreVTKOutput"
      plotFileRoot="phaseFieldPoro_debug"/>

    <Restart
      name="restartOutput"/>
  </Outputs>

  <Geometry>
    <Box
      name="fracture"
      xMin="{ 4.599, 4.975, -0.001 }"
      xMax="{ 5.401, 5.025,  0.101 }"/>

    <Box
      name="source"
      xMin="{ 4.899, 4.899, -0.001 }"
      xMax="{ 5.101, 5.101,  0.101 }"/>
  </Geometry>

</Problem>
