<?xml version="1.0" ?>

<Problem>
  <Solvers
    gravityVector="{ 0.0, 0.0, 0.0 }">
    <PhaseFieldPoromechanics
      name="phasefieldPoroSolve"
      poromechanicsSolverName="poroSolve"
      damageSolverName="damageSolve"
      couplingTypeOption="FixedStress"
      logLevel="1"
      subcycling="0"
      discretization="FE1"
      targetRegions="{ Region1 }">
    </PhaseFieldPoromechanics>

    <SinglePhasePoromechanics
      name="poroSolve"
      solidSolverName="solidMech"
      flowSolverName="singlePhaseFlow"
      logLevel="1"
      discretization="FE1"
      targetRegions="{ Region1 }"
      damageFlag="1">
      <NonlinearSolverParameters
        newtonMaxIter="100"
        newtonTol="1.0e-4"/>
      <LinearSolverParameters
        logLevel="0"
        solverType="gmres"
        preconditionerType="amg"/>
    </SinglePhasePoromechanics>

    <PhaseFieldDamageFEM
      name="damageSolve"
      discretization="FE1"
      timeIntegrationOption="SteadyState"
      fieldName="Damage"
      localDissipation="Linear"
      logLevel="1"
      targetRegions="{ Region1 }">
      <NonlinearSolverParameters
        newtonMaxIter="200"
        newtonTol="1.0e-6"/>
      <LinearSolverParameters
        logLevel="0"
        solverType="gmres"
        preconditionerType="amg"/>
    </PhaseFieldDamageFEM>

    <SolidMechanicsLagrangianSSLE
      name="solidMech"
      timeIntegrationOption="QuasiStatic"
      logLevel="1"
      discretization="FE1"
      targetRegions="{ Region1 }"/>

    <SinglePhaseFVM
      name="singlePhaseFlow"
      logLevel="1"
      discretization="singlePhaseTPFA"
      targetRegions="{ Region1 }"/>
  </Solvers>

  <Mesh>
    <VTKMesh 
      name="wellbore"
      regionAttribute="attribute"
      file="WellboreMesh/wellboreCoarse.vtu"/>
  </Mesh>

  <Events
    maxTime="1.0">
 
    <PeriodicEvent
      name="solverApplications"
      forceDt="1.0"
      target="/Solvers/phasefieldPoroSolve"/>

    <PeriodicEvent
      name="vtkOutput"
      timeFrequency="0.01"
      target="/Outputs/WellboreVTKOutput"/>

    <PeriodicEvent
      name="restarts"
      timeFrequency="0.5"
      targetExactTimestep="0"
      target="/Outputs/restartOutput"/>
  </Events>

  <NumericalMethods>
    <FiniteElements>
      <FiniteElementSpace 
        name="FE1"
        order="1"/>
    </FiniteElements>
    <FiniteVolume>
      <TwoPointFluxApproximation 
        name="singlePhaseTPFA"/>
    </FiniteVolume>
  </NumericalMethods>

  <ElementRegions>
    <CellElementRegion
      name="Region1"
      cellBlocks="{ 6_hexahedra, 7_hexahedra }"
      materialList="{ rock, water }"/>
  </ElementRegions>

  <Constitutive>
    <PorousDamageElasticIsotropic
      name="rock"
      solidModelName="rockSolid"
      porosityModelName="rockPorosity"
      permeabilityModelName="rockPerm"/>

    <DamageElasticIsotropic
      name="rockSolid"
      defaultDensity="2700"
      defaultBulkModulus="4.4144e3"
      defaultShearModulus="4.3363e3"
      lengthScale="1.0"
      criticalFractureEnergy="9.1e-2"
      criticalStrainEnergy="0"
      extDrivingForceSwitch="True"
      tensileStrength="27.0"
      compressiveStrength="77.0"
      deltaCoefficient="0.0"/>

    <BiotPorosity
      name="rockPorosity"
      grainBulkModulus="1.0e21"
      defaultReferencePorosity="0.375"/>

    <DamagePermeability
      name="rockPerm"
      bulkPermeability="1.0e-12"
      fracturePermeability="1.0e-9"/>

    <CompressibleSinglePhaseFluid
      name="water"
      defaultDensity="1e-9"
      defaultViscosity="1.0e-9"
      referencePressure="0.000"
      referenceDensity="1e-9"
      compressibility="4.4e-4"
      referenceViscosity="1.0e-9"
      viscosibility="0.0"/>
  </Constitutive>

  <FieldSpecifications>
    <FieldSpecification 
      name="Sh"
      initialCondition="1"
      setNames="{ all }"
      objectPath="ElementRegions/Region1"
      fieldName="rockSolid_stress"
      component="0"
      scale="-11.0"/>

    <FieldSpecification 
      name="SH"
      initialCondition="1"
      setNames="{ all }"
      objectPath="ElementRegions/Region1"
      fieldName="rockSolid_stress"
      component="1"
      scale="-14.0"/>

    <FieldSpecification 
      name="Sv"
      initialCondition="1"
      setNames="{ all }"
      objectPath="ElementRegions/Region1"
      fieldName="rockSolid_stress"
      component="2"
      scale="-17.0"/>
    
    <FieldSpecification 
      name="initialPressure"
      initialCondition="1"
      setNames="{all}"
      objectPath="ElementRegions/Region1"
      fieldName="pressure"
      scale="0.0"
    />

    <SourceFlux
      name="sourceTerm"
      objectPath="ElementRegions/Region1/6_hexahedra"
      scale="-8e-9"
      component="0"
      setNames="{ all }"
      functionName="timeFunction"/>
  </FieldSpecifications>

  <Functions>
    <TableFunction
      name="timeFunction"
      inputVarNames="{ time }"
      coordinates="{ 0, 1 }"
      values="{ 0.0, 40.0 }"/>
  </Functions>

  <Outputs>
    <VTK
      name="WellboreVTKOutput"
      plotFileRoot="wellborePoroNucleation_debug"/>

    <Restart
      name="restartOutput"/>
  </Outputs>

</Problem>
