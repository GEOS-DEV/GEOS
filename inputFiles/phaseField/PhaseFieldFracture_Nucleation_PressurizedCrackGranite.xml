<?xml version="1.0" ?>

<Problem>
  <Solvers
    gravityVector="{ 0.0, 0.0, 0.0 }">
    <PhaseFieldFracture
      name="PhaseFieldSolve"
      solidSolverName="SolidMechSolve"
      damageSolverName="DamageSolve"
      logLevel="1"
      targetRegions="{ Region1 }">
      <NonlinearSolverParameters
        couplingType="Sequential"
        subcycling="1"
        newtonMaxIter="200"/>
      <LinearSolverParameters
        directParallel="0"/>
    </PhaseFieldFracture>

    <SolidMechanicsLagrangianSSLE
      name="SolidMechSolve"
      timeIntegrationOption="QuasiStatic"
      logLevel="1"
      discretization="FE1"
      targetRegions="{ Region1 }"
      pressurizedDamageFlag="1">
      <NonlinearSolverParameters
        newtonMaxIter="200"
        newtonTol="1.0e-6"/>
      <LinearSolverParameters
        solverType="gmres"
        preconditionerType="amg"/>
    </SolidMechanicsLagrangianSSLE>

    <PhaseFieldDamageFEM
      name="DamageSolve"
      discretization="FE1"
      timeIntegrationOption="SteadyState"
      fieldName="Damage"
      
      localDissipation="Linear"
      irreversibilityFlag="1"
      damageUpperBound="0.99"
      logLevel="1"
      targetRegions="{ Region1 }">
      <NonlinearSolverParameters
        newtonMaxIter="200"
        newtonTol="1.0e-4"/>
      <LinearSolverParameters
        solverType="gmres"
        preconditionerType="amg"/>
    </PhaseFieldDamageFEM>
  </Solvers>

  <Mesh>
    <InternalMesh
      name="mesh1"
      elementTypes="{ C3D8 }"
      xCoords="{ 0, 50 }"
      yCoords="{ 0, 46, 50, 54, 100 }"
      zCoords="{ 0, 1 }"
      nx="{ 200 }"
      ny="{ 10, 16, 16, 10 }"
      nz="{ 1 }"
      cellBlockNames="{ cb1 }"/>
  </Mesh>

  <Events
    maxTime="18.0">
 
    <PeriodicEvent
      name="solverApplication0"
      endTime="18.0"
      forceDt="0.1"
      target="/Solvers/PhaseFieldSolve"/>

    <PeriodicEvent
      name="vtkOutput0"
      endTime="18.0"
      forceDt="0.1"
      target="/Outputs/VTKOutput"/>

    <PeriodicEvent
      name="restarts"
      timeFrequency="100.0"
      targetExactTimestep="0"
      target="/Outputs/restartOutput"/>
  </Events>

  <NumericalMethods>
    <FiniteElements>
      <FiniteElementSpace
        name="FE1"
        order="1"/>
    </FiniteElements>
  </NumericalMethods>

  <ElementRegions>
    <CellElementRegion
      name="Region1"
      cellBlocks="{ cb1 }"
      materialList="{ concrete }"/>

    <SurfaceElementRegion
      name="Fracture"
      defaultAperture="0.0"
      materialList="{ concrete }"/>
  </ElementRegions>

  <Constitutive>
    <DamageElasticIsotropic
      name="concrete"
      defaultDensity="2.7e-9"
      defaultBulkModulus="2.57e4"
      defaultShearModulus="1.64e4"
      lengthScale="1.0"
      criticalFractureEnergy="10e-3"
      criticalStrainEnergy="0"
      extDrivingForceFlag="1"
      degradationLowerLimit="1e-6"
      tensileStrength="13.0"
      compressiveStrength="120.0"
      deltaCoefficient="3.61"
      damagePressure="0.2906118338"/>
  </Constitutive>

  <FieldSpecifications>    
    <FieldSpecification 
      name="initialDamage"
      setNames="{ fracture }"
      objectPath="nodeManager"
      fieldName="Damage"
      scale="1.0"/>

    <FieldSpecification
      name="fixedY"
      fieldName="totalDisplacement"
      component="1"
      objectPath="nodeManager"
      scale="0.0"
      setNames="{ xpos, ypos, yneg }"/>

    <FieldSpecification
      name="fixedX"
      fieldName="totalDisplacement"
      component="0"
      objectPath="nodeManager"
      scale="0.0"
      setNames="{ ypos, yneg, xpos, xneg }"/>

    <FieldSpecification
      name="fixedZ"
      fieldName="totalDisplacement"
      component="2"
      objectPath="nodeManager"
      scale="0.0"
      setNames="{ zpos, zneg }"/> 
  </FieldSpecifications>

  <Outputs>
    <VTK
      name="VTKOutput"
      plotFileRoot="nucleation_pressurizedCrack_granite_l=1_h=0p25"/>

    <Restart
      name="restartOutput"/>
  </Outputs>

  <Geometry>
    <Box
      name="fracture"
      xMin="{ -0.001, 49.999, -0.001 }"
      xMax="{ 5.001, 50.001,  1.001 }"/>
  </Geometry>

</Problem>
