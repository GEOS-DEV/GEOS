<?xml version="1.0" ?>

<Problem>
  <Solvers
    gravityVector="{ 0.0, 0.0, 0.0 }">
    <PhaseFieldFracture
      name="PhaseFieldSolve"
      solidSolverName="SolidMechSolve"
      damageSolverName="DamageSolve"
      couplingTypeOption="FixedStress"
      logLevel="2"
      subcycling="0"
      discretization="FE1"
      targetRegions="{ Region1 }">
      <NonlinearSolverParameters
        newtonMaxIter="100"/>
      <LinearSolverParameters
        directParallel="0"/>
    </PhaseFieldFracture>

    <SolidMechanicsLagrangianSSLE
      name="SolidMechSolve"
      timeIntegrationOption="QuasiStatic"
      logLevel="1"
      discretization="FE1"
      targetRegions="{ Region1 }"
      >
      <NonlinearSolverParameters
        newtonMaxIter="200"
        newtonTol="1.0e-10"/>
      <LinearSolverParameters
        directParallel="0"/>
    </SolidMechanicsLagrangianSSLE>

    <PhaseFieldDamageFEM
      name="DamageSolve"
      discretization="FE1"
      timeIntegrationOption="SteadyState"
      fieldName="Damage"
      
      localDissipation="Linear"
      logLevel="1"
      targetRegions="{ Region1 }">
      <NonlinearSolverParameters
        newtonMaxIter="200"
        newtonTol="1.0e-10"/>
      <LinearSolverParameters
        directParallel="0"/>
    </PhaseFieldDamageFEM>
  </Solvers>

  <Mesh>
    <PAMELAMesh
      name="3PointsTests"
      file="3PointsTestsMesh/L-shape.msh"/>
  </Mesh>

  <Events
    maxTime="100.0">
    <!-- <SoloEvent name="siloOutput0"
               target="/Outputs/SiloOutput"/> -->

    <!-- This event is applied every cycle, and overrides the
    solver time-step request -->
    <PeriodicEvent
      name="solverApplications"
      forceDt="1.0"
      target="/Solvers/PhaseFieldSolve"/>

    <!-- This event is applied every 1.0s.  The targetExactTimestep
    flag allows this event to request a dt modification to match an
    integer multiple of the timeFrequency. -->
    <PeriodicEvent
      name="vtkOutput"
      timeFrequency="1.0"
      target="/Outputs/DamageNucleation3PointsTest_VTKOutput"/>

    <PeriodicEvent
      name="siloOutputs"
      timeFrequency="1.0"
      target="/Outputs/DamageNucleation3PointsTest_SiloOutput"/>

    <PeriodicEvent
      name="restarts"
      timeFrequency="50"
      targetExactTimestep="0"
      target="/Outputs/restartOutput"/>
  </Events>

  <NumericalMethods>
    <FiniteElements>
      <FiniteElementSpace
        name="FE1"
        order="1"/>
    </FiniteElements>
  </NumericalMethods>

  <ElementRegions>
    <CellElementRegion
      name="Region1"
      cellBlocks="{ all_HEX }"
      materialList="{ PMMA }"/>

  </ElementRegions>

  <Constitutive>
    <DamageElasticIsotropic
      name="PMMA"
      defaultDensity="2700"
      defaultBulkModulus="8.42e3"
      defaultShearModulus="1.80e3"
      lengthScale="0.05"
      criticalFractureEnergy="0.48"
      criticalStrainEnergy="0"
      extDrivingForceSwitch="True"
      tensileStrength="128.0"
      compressiveStrength="256.0"
      deltaCoefficient="0.5"/>
  </Constitutive>

  <FieldSpecifications>
    <FieldSpecification
      name="yDisp"
      fieldName="TotalDisplacement"
      component="1"
      objectPath="nodeManager"
      scale="-1e-2"
      functionName="disp"
      setNames="{ forcepoint }"/>

    <FieldSpecification
      name="fixedY"
      fieldName="TotalDisplacement"
      component="1"
      objectPath="nodeManager"
      scale="0.0"
      setNames="{ leftpin, rightpin }"/>

    <FieldSpecification
      name="fixedX"
      fieldName="TotalDisplacement"
      component="0"
      objectPath="nodeManager"
      scale="0.0"
      setNames="{ leftpin }"/>

    <FieldSpecification
      name="fixedZ"
      fieldName="TotalDisplacement"
      component="2"
      objectPath="nodeManager"
      scale="0.0"
      setNames="{ frontface }"/>

  </FieldSpecifications>

  <Functions>
    <TableFunction
      name="disp"
      inputVarNames="{ time }"
      coordinates="{ 0, 100 }"
      values="{ 0, 100 }"/>
  </Functions>

  <Outputs>
    <Silo
      name="DamageNucleation3PointsTest_SiloOutput"/>

    <VTK
      name="DamageNucleation3PointsTest_VTKOutput"
      plotFileRoot="plot_damage_3points"/>

    <Restart
      name="restartOutput"/>
  </Outputs>

   <Geometry>
    <Box
      name="leftpin"
      xMin="{ 44.999, -0.001, -0.001 }"
      xMax="{ 45.001, 0.001, 1.001 }"/>

    <Box
      name="rightpin"
      xMin="{ 118.999, 13.999, -0.001 }"
      xMax="{ 119.001, 14.001, 1.001 }"/>

    <Box
      name="forcepoint"
      xMin="{ 71.999, 27.999, -0.001 }"
      xMax="{ 72.001, 28.001, 1.001 }"/>

    <Box
      name="frontface"
      xMin="{ -0.001, -0.001, 0.999 }"
      xMax="{ 126.001, 28.001, 1.001 }"/>
  </Geometry>
</Problem>
