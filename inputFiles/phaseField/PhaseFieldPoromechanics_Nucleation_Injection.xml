<?xml version="1.0" ?>

<Problem>
  <Solvers
    gravityVector="{ 0.0, 0.0, 0.0 }">
    <PhaseFieldPoromechanics
      name="phasefieldPoroSolve"
      poromechanicsSolverName="poroSolve"
      damageSolverName="damageSolve"
      logLevel="1"
      targetRegions="{ Region1 }">
    </PhaseFieldPoromechanics>

    <SinglePhasePoromechanics
      name="poroSolve"
      solidSolverName="solidMech"
      flowSolverName="singlePhaseFlow"
      logLevel="1"
      targetRegions="{ Region1 }"
      damageFlag="1">
      <NonlinearSolverParameters
        newtonMaxIter="100"
        newtonTol="1.0e-6"/>
      <LinearSolverParameters
        solverType="gmres"
        preconditionerType="mgr"/>
    </SinglePhasePoromechanics>

    <PhaseFieldDamageFEM
      name="damageSolve"
      discretization="FE1"
      timeIntegrationOption="SteadyState"
      fieldName="Damage"
      localDissipation="Linear"
      irreversibilityFlag="1"
      damageUpperBound="0.99"
      fracturePressureTermFlag="1"
      logLevel="1"
      targetRegions="{ Region1 }">
      <NonlinearSolverParameters
        newtonMaxIter="200"
        newtonTol="1.0e-4"/>
      <LinearSolverParameters
        solverType="direct"/>
    </PhaseFieldDamageFEM>

    <SolidMechanicsLagrangianSSLE
      name="solidMech"
      timeIntegrationOption="QuasiStatic"
      logLevel="1"
      discretization="FE1"
      targetRegions="{ Region1 }"/>

    <SinglePhaseHybridFVM
      name="singlePhaseFlow"
      logLevel="1"
      discretization="singlePhaseHybridMimetic"
      targetRegions="{ Region1 }"/>
  </Solvers>

  <Mesh>
    <InternalMesh
      name="mesh1"
      elementTypes="{ C3D8 }"
      xCoords="{ 0, 100 }"
      yCoords="{ 0, 49.9, 50.1, 100 }"
      zCoords="{ 0, 0.1 }"
      nx="{ 251 }"
      ny="{ 125, 1, 125 }"
      nz="{ 1 }"
      cellBlockNames="{ cb1 }"/>
  </Mesh>

  <Events
    maxTime="1000.5">

    <PeriodicEvent
      name="solverApplication0"
      endTime="1000.0"
      forceDt="1000.0"
      target="/Solvers/phasefieldPoroSolve"/>

    <PeriodicEvent
      name="solverApplication1"
      beginTime="1000.0"
      endTime="1000.5"
      forceDt="0.01"
      target="/Solvers/phasefieldPoroSolve"/>

    <PeriodicEvent
      name="vtkOutput0"
      endTime="1000.0"
      forceDt="1000.0"
      target="/Outputs/VTKOutput"/>

    <PeriodicEvent
      name="vtkOutput1"
      beginTime="1000.0"
      endTime="1000.5"
      forceDt="0.01"
      target="/Outputs/VTKOutput"/>

    <PeriodicEvent
      name="restarts"
      timeFrequency="500"
      targetExactTimestep="0"
      target="/Outputs/restartOutput"/>
  </Events>

  <NumericalMethods>
    <FiniteElements>
      <FiniteElementSpace 
        name="FE1"
        order="1"/>
    </FiniteElements>
    <FiniteVolume>
      <HybridMimeticDiscretization
        name="singlePhaseHybridMimetic"
        innerProductType="quasiTPFA"/>
    </FiniteVolume>
  </NumericalMethods>

  <ElementRegions>
    <CellElementRegion
      name="Region1"
      cellBlocks="{ cb1 }"
      materialList="{ rock, water }"/>
  </ElementRegions>

  <Constitutive>
    <PorousDamageElasticIsotropic
      name="rock"
      solidModelName="rockSolid"
      porosityModelName="rockPorosity"
      permeabilityModelName="rockPerm"/>

    <DamageElasticIsotropic
      name="rockSolid"
      defaultDensity="2700"
      defaultBulkModulus="16.666667e9"
      defaultShearModulus="12.5e9"
      lengthScale="1.0"
      criticalFractureEnergy="100"
      criticalStrainEnergy="0"
      extDrivingForceFlag="1"
      degradationLowerLimit="1e-6"
      tensileStrength="5.5e6"
      compressiveStrength="40.0e6"
      deltaCoefficient="0.44"/>

    <BiotPorosity
      name="rockPorosity"
      grainBulkModulus="83.333335e9"
      defaultReferencePorosity="0.1"/>

    <DamagePermeability
      name="rockPerm"
      bulkPermeability="1.0e-15"/>

    <CompressibleSinglePhaseFluid
      name="water"
      defaultDensity="1000"
      defaultViscosity="0.001"
      referencePressure="0.000"
      referenceDensity="1000"
      compressibility="0"
      referenceViscosity="0.001"
      viscosibility="0.0"/>
  </Constitutive>

  <FieldSpecifications>   
    <!-- <FieldSpecification 
      name="Sh"
      initialCondition="1"
      setNames="{ all }"
      objectPath="ElementRegions/Region1/cb1"
      fieldName="rockSolid_stress"
      component="1"
      scale="-8.0"/>

    <FieldSpecification 
      name="SH"
      initialCondition="1"
      setNames="{ all }"
      objectPath="ElementRegions/Region1/cb1"
      fieldName="rockSolid_stress"
      component="0"
      scale="-12.0"/>

    <FieldSpecification 
      name="Sv"
      initialCondition="1"
      setNames="{ all }"
      objectPath="ElementRegions/Region1/cb1"
      fieldName="rockSolid_stress"
      component="2"
      scale="0.0"/> -->

    <FieldSpecification 
      name="initialPressure"
      initialCondition="1"
      setNames="{all}"
      objectPath="ElementRegions/Region1/cb1"
      fieldName="pressure"
      scale="0.0"/>

    <FieldSpecification 
      name="initialDamage"
      setNames="{ fracture }"
      objectPath="nodeManager"
      fieldName="Damage"
      scale="1.0"/>

    <FieldSpecification
      name="fixedY"
      fieldName="totalDisplacement"
      component="1"
      objectPath="nodeManager"
      scale="0.0"
      setNames="{ ypos, yneg }"/>

    <FieldSpecification
      name="fixedX"
      fieldName="totalDisplacement"
      component="0"
      objectPath="nodeManager"
      scale="0.0"
      setNames="{ xpos, xneg }"/>

    <FieldSpecification
      name="fixedZ"
      fieldName="totalDisplacement"
      component="2"
      objectPath="nodeManager"
      scale="0.0"
      setNames="{ zpos, zneg }"/> 

    <FieldSpecification 
      name="pressureConstraint"
      setNames="{xminus, xplus, yminus, yplus}"
      objectPath="ElementRegions/Region1/cb1"
      fieldName="pressure"
      scale="0.0"/>

    <SourceFlux
      name="sourceTerm"
      objectPath="ElementRegions/Region1/cb1"
      scale="-4e-9"
      component="0"
      setNames="{ source }"
      functionName="timeFunction"/>
  </FieldSpecifications>

  <Functions>
    <TableFunction
      name="timeFunction"
      inputVarNames="{ time }"
      coordinates="{ 0, 1000.02, 1000.5 }"
      values="{ 0.0, 0.0, 0.5 }"/>
  </Functions>

  <Outputs>
    <VTK
      name="VTKOutput"
      plotFileRoot="phaseFieldPoro_nucleation_injection"/>

    <Restart
      name="restartOutput"/>
  </Outputs>

  <Geometry>
    <Box
      name="fracture"
      xMin="{ 45.999, 49.80, -0.001 }"
      xMax="{ 54.001, 50.20,  0.101 }"/>

    <Box
      name="source"
      xMin="{ 48.999, 48.999, -0.001 }"
      xMax="{ 51.001, 51.001,  0.101 }"/>

    <Box
      name="xplus"
      xMin="{ 99.599, -0.001, -0.1 }"
      xMax="{ 100.001, 100.001, 0.101 }"/>

    <Box
      name="xminus"
      xMin="{ -0.001, -0.001, -0.1 }"
      xMax="{ 0.401, 100.001, 0.101 }"/>

    <Box
      name="yplus"
      xMin="{ -0.001, 99.599, -0.1 }"
      xMax="{ 100.001, 100.001, 0.101 }"/>

    <Box
      name="yminus"
      xMin="{ -0.001, -0.001, -0.1 }"
      xMax="{ 100.001, 0.401, 0.101 }"/>
  </Geometry>

</Problem>
