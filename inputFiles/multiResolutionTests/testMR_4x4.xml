<?xml version="1.0" ?>

<Problem>
  <Solvers
    gravityVector="{ 0.0, 0.0, 0.0 }">

    <MultiResolutionFlowHF
      name="MultiResolutionSolve"
      baseSolverName="poroSolver"
      patchSolverName="PhaseFieldSolve"
      logLevel="1"
      discretization="FE1"
      initialBaseTip = "{0.0,0.0,0.0}"
      initialTipElementIndex="0"
      targetRegions="{base/base_cer,patch/patch_cer}">
      <NonlinearSolverParameters
        newtonMaxIter="10"/>
      <LinearSolverParameters
        logLevel="0"/>
    </MultiResolutionFlowHF>

    <SinglePhasePoromechanicsEmbeddedFractures
      name="poroSolver"
      targetRegions="{base/base_cer,base/Fracture_cer}"
      initialDt="1e-3"
      fracturesSolverName="fractureMechSolver"
      flowSolverName="flowSolver"
      solidSolverName="matrixMechSolver"
      logLevel="1">
      <NonlinearSolverParameters
        maxTimeStepCuts="3"/>
      <LinearSolverParameters
        directParallel="0"
        solverType="direct"/>
    </SinglePhasePoromechanicsEmbeddedFractures>

    <SolidMechanicsEmbeddedFractures
      name="fractureMechSolver"
      targetRegions="{base/base_cer,base/Fracture_cer}"
      solidSolverName="matrixMechSolver"
      fractureRegionName="Fracture_cer"
      contactRelationName="fractureContact"/>

    <SolidMechanicsLagrangianSSLE
      name="matrixMechSolver"
      timeIntegrationOption="QuasiStatic"
      discretization="FE1"
      targetRegions="{ base/base_cer }"/>

    <SinglePhaseFVM
      name="flowSolver"
      discretization="singlePhaseTPFA"
      targetRegions="{base/base_cer,base/Fracture_cer}"/>

    <EmbeddedSurfaceGenerator
      name="SurfaceGenerator"
      discretization="FE1"
      targetRegions="{base/base_cer,base/Fracture_cer}"
      fractureRegion="Fracture_cer"
      logLevel="2"
      mpiCommOrder="1"/>

    <PhaseFieldFracture
      name="PhaseFieldSolve"
      solidSolverName="PFSolidMechSolve"
      damageSolverName="DamageSolve"
      logLevel="1"
      pressureEffects="1"
      targetRegions="{patch/patch_cer}">
      <NonlinearSolverParameters
        newtonMaxIter="50"
        subcycling="1"
        couplingType="Sequential"/>
      <LinearSolverParameters
        logLevel="0"/>
    </PhaseFieldFracture>

    <SolidMechanicsLagrangianSSLE
      name="PFSolidMechSolve"
      timeIntegrationOption="QuasiStatic"
      logLevel="1"
      discretization="FE1"
      targetRegions="{patch/patch_cer}">
      <NonlinearSolverParameters
        newtonMaxIter="50"
        newtonTol="1.0e-6"/>
      <LinearSolverParameters
        solverType="gmres"
        preconditionerType="amg"/>
    </SolidMechanicsLagrangianSSLE>

    <PhaseFieldDamageFEM
      name="DamageSolve"
      discretization="FE1"
      timeIntegrationOption="SteadyState"
      localDissipation="Linear"
      logLevel="1"
      targetRegions="{patch/patch_cer}"
      useDamageViscosity="1"
      damageViscosityCoeff="10">
      <NonlinearSolverParameters
        newtonMaxIter="50"
        newtonTol="1.0e-8"/>
      <LinearSolverParameters
        solverType="gmres"
        preconditionerType="amg"/>
    </PhaseFieldDamageFEM>
  </Solvers>

  <Mesh>
    <InternalMesh
      name="base"
      elementTypes="{ C3D8 }"
      xCoords="{ 0, 3 }"
      yCoords="{ 0, 3 }"
      zCoords="{ 0, 1 }"
      nx="{ 4 }"
      ny="{ 4 }"
      nz="{ 4 }"
      cellBlockNames="{ cb1 }"/>

    <InternalMesh
      name="patch"
      elementTypes="{ C3D8 }"
      xCoords="{ 0, 3 }"
      yCoords="{ 0, 3 }"
      zCoords="{ 0, 1 }"
      nx="{ 8 }"
      ny="{ 8 }"
      nz="{ 8 }"
      cellBlockNames="{ cb2 }"/>    
  </Mesh>

  <Geometry>
    <Box
      name="source"
      xMin="{ 1.35, -0.01, -0.01 }"
      xMax="{ 1.65,  3.01,  1.01 }"/>
    <BoundedPlane
      name="FracturePlane"
      normal="{ 1, 0, 0 }"
      origin="{ 1.5, 1.5, 1.5 }"
      lengthVector="{ 0, 1, 0 }"
      widthVector="{ 0, 0, 1 }"
      dimensions="{ 20, 40 }"/>
  </Geometry>

  <Events
    maxTime="0.01">
    <SoloEvent
      name="preFracture"
      target="/Solvers/SurfaceGenerator"/>

    <PeriodicEvent
      name="solverApplications"
      forceDt="0.01"
      target="/Solvers/MultiResolutionSolve"/>      

    <PeriodicEvent
      name="outputs"
      cycleFrequency="1"
      target="/Outputs/vtkOutput"/>
  </Events>

  <NumericalMethods>
    <FiniteElements>
      <FiniteElementSpace
        name="FE1"
        order="1"/>
    </FiniteElements>

    <FiniteVolume>
      <TwoPointFluxApproximation
        name="singlePhaseTPFA"/>
    </FiniteVolume>
  </NumericalMethods>

  <ElementRegions>
    <CellElementRegion
      name="base_cer"
      meshBody="base"
      cellBlocks="{ cb1 }"
      materialList="{ porousRock, water }"/>
    <CellElementRegion
      name="patch_cer"
      meshBody="patch"
      cellBlocks="{ cb2 }"
      materialList="{ damagedRock }"/>
    <SurfaceElementRegion
      name="Fracture_cer"
      faceBlock="embeddedSurfaceSubRegion"
      subRegionType="embeddedElement"
      materialList="{ water, fractureFilling, fractureContact }"
      defaultAperture="1.0e-3"
	    meshBody="base"/>
    <SurfaceElementRegion
      name="pFracture_cer"
      subRegionType="embeddedElement"
      materialList="{ fractureContact }"
      defaultAperture="1.0e-3"
	    meshBody="patch"/>  
  </ElementRegions>

  <Constitutive>
    <PorousElasticIsotropic
      name="porousRock"
      solidModelName="rock"
      porosityModelName="rockPorosity"
      permeabilityModelName="rockPerm"/>

    <ElasticIsotropic
      name="rock"
      defaultDensity="1"
      defaultBulkModulus="5555555555.5556"
      defaultShearModulus="4166666666.6667"/>

    <DamageElasticIsotropic
      name="damagedRock"
      defaultDensity="2700"
      defaultBulkModulus="5555555555.5556"
      defaultShearModulus="4166666666.6667"
      lengthScale="0.2"
      degradationLowerLimit="1e-6"
      defaultCriticalFractureEnergy="2.7e5"
      criticalStrainEnergy="16.875"/>    

    <ConstantPermeability
      name="rockPerm"
      permeabilityComponents="{ 1.0e-12, 1.0e-12, 1.0e-12 }"/>

    <CompressibleSinglePhaseFluid
      name="water"
      defaultDensity="1000"
      defaultViscosity="1.0e-3"
      referencePressure="1e6"
      compressibility="0.0e0"
      referenceViscosity="1e-3"
      viscosibility="0.0"/>

    <BiotPorosity
      name="rockPorosity"
      grainBulkModulus="1.0e27"
      defaultReferencePorosity="0.2"/>

    <CompressibleSolidParallelPlatesPermeability
      name="fractureFilling"
      solidModelName="nullSolid"
      porosityModelName="fracturePorosity"
      permeabilityModelName="fracturePerm"/>

    <NullModel
      name="nullSolid"/>

    <PressurePorosity
      name="fracturePorosity"
      defaultReferencePorosity="1.00"
      referencePressure="0.0"
      compressibility="0.0"/>

    <ParallelPlatesPermeability
      name="fracturePerm"/>

    <FrictionlessContact
      name="fractureContact"
      penaltyStiffness="0.0e8"
      apertureTableName="apertureTable"/>
    
  </Constitutive>

  <FieldSpecifications>

    <FieldSpecification
      name="fracPorosity"
      initialCondition="1"
      setNames="{ all }"
      objectPath="base/Level0/ElementRegions/Fracture_cer"
      fieldName="fracturePorosity_porosity"
      scale="1.00"/>

    <FieldSpecification
      name="initialPressure"
      initialCondition="0"
      setNames="{ all }"
      objectPath="ElementRegions/base_cer"
      fieldName="pressure"
      scale="0.0"/>

    <FieldSpecification
      name="initialPressureFracture"
      initialCondition="1"
      setNames="{ all }"
      objectPath="ElementRegions/Fracture_cer"
      fieldName="pressure"
      scale="10.0"/>

    <FieldSpecification
      name="xnegconstraint"
      objectPath="nodeManager"
      fieldName="totalDisplacement"
      component="0"
      scale="0.0"
      setNames="{ xneg, xpos }"/>

    <FieldSpecification
      name="yposconstraint"
      objectPath="nodeManager"
      fieldName="totalDisplacement"
      component="1"
      scale="1.0e-9"
      setNames="{ ypos }"/>

   <FieldSpecification
      name="ynegconstraint"
      objectPath="nodeManager"
      fieldName="totalDisplacement"
      component="1"
      scale="0.0e-6"
      setNames="{ yneg }"/>    

    <FieldSpecification
      name="zconstraint"
      objectPath="nodeManager"
      fieldName="totalDisplacement"
      component="2"
      scale="0.0"
      setNames="{ zneg, zpos }"/>

    <FieldSpecification
      name="leftPressure"
      objectPath="faceManager"
      fieldName="pressure"
      scale="0.0"
      setNames="{ xneg }"/>

    <FieldSpecification
      name="rightPressure"
      objectPath="faceManager"
      fieldName="pressure"
      scale="0.0"
      setNames="{ xpos }"/>

    <SourceFlux
      name="sourceTerm"
      objectPath="ElementRegions/Fracture_cer"
      scale="-5e-4"
      setNames="{ source }"/>

  </FieldSpecifications>

  <Functions>
    <TableFunction
      name="apertureTable"
      coordinates="{ -1.0e-3, 0.0 }"
      values="{ 1.0e-6, 1.0e-3 }"/>
  </Functions>
  
  <Outputs>
    <VTK
      name="vtkOutput"
      plotFileRoot="meshNumbering_4x4"
      plotLevel="2"
      format="ascii"/>

    <Restart
      name="restartOutput"/>
  </Outputs>
</Problem>
