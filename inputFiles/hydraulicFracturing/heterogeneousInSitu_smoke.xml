<?xml version="1.0" ?>

<Problem>
  <Mesh>
    <InternalMesh
      name="mesh1"
      xCoords="{ 0, 200, 250 }"
      yCoords="{ -100, 0, 100 }"
      zCoords="{ -150, -100, 0, 100, 150 }"
      nx="{ 20, 5 }"
      ny="{ 2, 2 }"
      nz="{ 1, 5, 5, 1 }"
      xBias="{ 0, -0.6 }"
      yBias="{ 0.6, -0.6 }"
      zBias="{ 0.6, 0, 0, -0.6 }"
      cellBlockNames="{ cb1 }"
      elementTypes="{ C3D8 }"/>
  </Mesh>

  <Geometry>
    <Box
      name="source_a"
      xMin="{ -0.1, -0.1, -0.1 }"
      xMax="{ 10.1, 0.1, 20.1 }"/>

    <Box
      name="perf_a"
      xMin="{ -10.1, -0.1, -20.1 }"
      xMax="{ 10.1, 0.1, 20.1 }"/>

    <ThickPlane
      name="fracturable_a"
      normal="{ 0, 1, 0 }"
      origin="{ 0, 0, 0 }"
      thickness="0.1"/>
  </Geometry>

  <Solvers
    gravityVector="{ 0.0, 0.0, -9.81 }">
    <Hydrofracture
      name="hydrofracture"
      solidSolverName="lagsolve"
      flowSolverName="SinglePhaseFlow"
      surfaceGeneratorName="SurfaceGen"
      logLevel="1"
      targetRegions="{ Fracture }"
      contactRelationName="fractureContact"
      maxNumResolves="2">
      <NonlinearSolverParameters
        newtonTol="1.0e-6"
        newtonMaxIter="40"
        lineSearchMaxCuts="3"/>
      <LinearSolverParameters
        directParallel="0"/>
    </Hydrofracture>

    <SolidMechanicsLagrangianSSLE
      name="lagsolve"
      timeIntegrationOption="QuasiStatic"
      discretization="FE1"
      targetRegions="{ Domain, Fracture }"
      contactRelationName="fractureContact">
    </SolidMechanicsLagrangianSSLE>

    <SinglePhaseFVM
      name="SinglePhaseFlow"
      discretization="singlePhaseTPFA"
      targetRegions="{ Fracture }">
    </SinglePhaseFVM>

    <SurfaceGenerator
      name="SurfaceGen"
      targetRegions="{ Domain }"
      rockToughness="2.236068e6"
      nodeBasedSIF="1"
      mpiCommOrder="1"/>
  </Solvers>

  <Outputs>
    <VTK
      name="vtkOutput"/>

    <Silo
      name="siloOutput"/>

    <Restart
      name="restartOutput"/>
  </Outputs>

  <Events
    maxTime="60.4"
    logLevel="1">
    <SoloEvent
      name="preFracture"
      target="/Solvers/SurfaceGen"/>

    <PeriodicEvent
      name="outputs_vtk"
      timeFrequency="6e1"
      targetExactTimestep="0"
      target="/Outputs/vtkOutput"/>

    <PeriodicEvent
      name="outputs_silo"
      timeFrequency="6e1"
      targetExactTimestep="0"
      target="/Outputs/siloOutput"/>

    <PeriodicEvent
      name="solverApplications"
      maxEventDt="3e1"
      target="/Solvers/hydrofracture"/>

    <PeriodicEvent
      name="pumpStart"
      beginTime="6e1"
      endTime="6.5e1"
      maxEventDt="2e-1"/>

    <PeriodicEvent
      name="restarts"
      timeFrequency="6e1"
      target="/Outputs/restartOutput"/>
  </Events>

  <ElementRegions>
    <CellElementRegion
      name="Domain"
      cellBlocks="{ cb1 }"
      materialList="{ water, rock }"/>

    <SurfaceElementRegion
      name="Fracture"
      defaultAperture="1.0e-4"
      materialList="{ water, fractureFilling, fractureContact }"/>
  </ElementRegions>

  <Constitutive>
    <CompressibleSinglePhaseFluid
      name="water"
      defaultDensity="1000"
      defaultViscosity="2.5e-2"
      referencePressure="0.0"
      compressibility="5e-10"
      referenceViscosity="2.5e-2"
      viscosibility="0.0"/>

    <ElasticIsotropic
      name="rock"
      defaultDensity="2700"
      defaultBulkModulus="1.0e9"
      defaultShearModulus="1.0e9"/>

    <CompressibleSolidParallelPlatesPermeability
      name="fractureFilling"
      solidModelName="nullSolid"
      porosityModelName="fracturePorosity"
      permeabilityModelName="fracturePerm"/>

    <NullModel
      name="nullSolid"/>

    <PressurePorosity
      name="fracturePorosity"
      defaultReferencePorosity="1.00"
      referencePressure="0.0"
      compressibility="0.0"/>

    <FrictionlessContact
      name="fractureContact"
      penaltyStiffness="0.0e8"
      apertureTableName="apertureTable"/>

    <ParallelPlatesPermeability
      name="fracturePerm"/>
  </Constitutive>

  <NumericalMethods>
    <FiniteElements>
      <FiniteElementSpace
        name="FE1"
        order="1"/>
    </FiniteElements>

    <FiniteVolume>
      <TwoPointFluxApproximation
        name="singlePhaseTPFA"
        meanPermCoefficient="1.0"/>
    </FiniteVolume>
  </NumericalMethods>

  <Functions>
    <TableFunction
      name="flow_rate"
      inputVarNames="{ time }"
      coordinateFiles="{ ./tables/flowRate_time.csv }"
      voxelFile="./tables/flowRate.csv"/>

    <TableFunction
      name="sigma_xx"
      inputVarNames="{ elementCenter }"
      coordinateFiles="{ ./tables/x.csv, ./tables/y.csv, ./tables/z.csv }"
      voxelFile="./tables/sigma_xx.csv"
      interpolation="upper"/>

    <TableFunction
      name="sigma_yy"
      inputVarNames="{ elementCenter }"
      coordinateFiles="{ ./tables/x.csv, ./tables/y.csv, ./tables/z.csv }"
      voxelFile="./tables/sigma_yy.csv"
      interpolation="upper"/>

    <TableFunction
      name="sigma_zz"
      inputVarNames="{ elementCenter }"
      coordinateFiles="{ ./tables/x.csv, ./tables/y.csv, ./tables/z.csv }"
      voxelFile="./tables/sigma_zz.csv"
      interpolation="upper"/>

    <TableFunction
      name="init_pressure"
      inputVarNames="{ elementCenter }"
      coordinateFiles="{ ./tables/x.csv, ./tables/y.csv, ./tables/z.csv }"
      voxelFile="./tables/porePressure.csv"
      interpolation="upper"/>

    <TableFunction
      name="bulk_modulus"
      inputVarNames="{ elementCenter }"
      coordinateFiles="{ ./tables/x.csv, ./tables/y.csv, ./tables/z.csv }"
      voxelFile="./tables/bulkModulus.csv"
      interpolation="upper"/>

    <TableFunction
      name="shear_modulus"
      inputVarNames="{ elementCenter }"
      coordinateFiles="{ ./tables/x.csv, ./tables/y.csv, ./tables/z.csv }"
      voxelFile="./tables/shearModulus.csv"
      interpolation="upper"/>

    <TableFunction
      name="apertureTable"
      coordinates="{ -1.0e-3, 0.0 }"
      values="{ 1.0e-6, 1.0e-4 }"/>
  </Functions>

  <!--<Included name="primary_includes"><File name="./heterogeneousInSitu_base.xml"/></Included>-->
  <!--<Parameters><Parameter name="Nperf" value="5"/><Parameter name="Nswarm" value="5"/><Parameter name="mu_init" value="0.001"/><Parameter name="K_init" value="1e6"/><Parameter name="mu_upscaled" value="2.5e-2"/><Parameter name="K_upscaled" value="2.236068e6"/><Parameter name="ContactStiffness" value="1e10"/><Parameter name="pump_start" value="6e1"/><Parameter name="pump_ramp" value="5"/><Parameter name="pump_ramp_dt_limit" value="2e-1"/><Parameter name="dt_max" value="3e1"/><Parameter name="t_max" value="1.2e3"/><Parameter name="table_root" value="./tables"/><Parameter name="t_allocation" value="1.68e3"/></Parameters>-->
  <FieldSpecifications>
    <FieldSpecification
      name="frac"
      fieldName="ruptureState"
      initialCondition="1"
      objectPath="faceManager"
      scale="1"
      setNames="{ perf_a }"/>

    <FieldSpecification
      name="separableFace"
      fieldName="isFaceSeparable"
      initialCondition="1"
      objectPath="faceManager"
      scale="1"
      setNames="{ fracturable_a }"/>

    <FieldSpecification
      name="waterDensity"
      fieldName="water_density"
      initialCondition="1"
      objectPath="ElementRegions"
      scale="1000"
      setNames="{ perf_a }"/>

    <FieldSpecification
      name="bulk_modulus"
      initialCondition="1"
      setNames="{ all }"
      objectPath="ElementRegions/Domain"
      fieldName="rock_bulkModulus"
      functionName="bulk_modulus"
      scale="1.0"/>

    <FieldSpecification
      name="shear_modulus"
      initialCondition="1"
      setNames="{ all }"
      objectPath="ElementRegions/Domain"
      fieldName="rock_shearModulus"
      functionName="shear_modulus"
      scale="1.0"/>

    <FieldSpecification
      name="sigma_xx"
      initialCondition="1"
      setNames="{ all }"
      objectPath="ElementRegions/Domain"
      fieldName="rock_stress"
      component="0"
      functionName="sigma_xx"
      scale="1.0"/>

    <FieldSpecification
      name="sigma_yy"
      initialCondition="1"
      setNames="{ all }"
      objectPath="ElementRegions/Domain"
      fieldName="rock_stress"
      component="1"
      functionName="sigma_yy"
      scale="1.0"/>

    <FieldSpecification
      name="sigma_zz"
      initialCondition="1"
      setNames="{ all }"
      objectPath="ElementRegions/Domain"
      fieldName="rock_stress"
      component="2"
      functionName="sigma_zz"
      scale="1.0"/>

    <FieldSpecification
      name="x_constraint"
      component="0"
      fieldName="totalDisplacement"
      objectPath="nodeManager"
      scale="0.0"
      setNames="{ xneg, xpos }"/>

    <FieldSpecification
      name="y_constraint"
      component="1"
      fieldName="totalDisplacement"
      objectPath="nodeManager"
      scale="0.0"
      setNames="{ yneg, ypos }"/>

    <FieldSpecification
      name="z_constraint"
      component="2"
      fieldName="totalDisplacement"
      objectPath="nodeManager"
      scale="0.0"
      setNames="{ zneg, zpos }"/>

    <SourceFlux
      name="sourceTerm"
      objectPath="ElementRegions/Fracture"
      scale="-2e2"
      functionName="flow_rate"
      setNames="{ source_a }"/>
  </FieldSpecifications>
</Problem>
