		</xsd:choice>
		<xsd:attribute name="cellBlocks" type="string_array" use="required" />
		<xsd:attribute name="ghostRank" type="integer_array" />
		<xsd:attribute name="material" type="string" use="required" />
		<xsd:attribute name="materialList" type="string_array" use="required" />
