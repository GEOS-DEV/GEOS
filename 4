	<xsd:element name="Problem" type="ProblemType" />
	<xsd:complexType name="ProblemType">
		<xsd:choice maxOccurs="unbounded">
			<xsd:element name="Events" type="EventsType" />
			<xsd:element name="Geometry" type="GeometryType" maxOccurs="1" />
			<xsd:element name="Mesh" type="MeshType" maxOccurs="1" />
			<xsd:element name="NumericalMethods" type="NumericalMethodsType" />
			<xsd:element name="Outputs" type="OutputsType" maxOccurs="1" />
			<xsd:element name="Sets" type="SetsType" />
			<xsd:element name="Solvers" type="SolversType" maxOccurs="1" />
			<xsd:element name="Domain" type="DomainType" maxOccurs="1" />
			<xsd:element name="neighborData" type="neighborDataType" />
		</xsd:choice>
		<xsd:attribute name="ghostRank" type="integer_array" />
	</xsd:complexType>
	<xsd:complexType name="EventsType">
