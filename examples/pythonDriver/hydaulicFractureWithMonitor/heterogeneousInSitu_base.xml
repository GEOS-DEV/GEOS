<?xml version="1.0" ?>

<Problem>
  <Solvers
    gravityVector="0.0,0.0,-9.81">
    <Hydrofracture
      name="hydrofracture"
      solidSolverName="lagsolve"
      logLevel="1"
      contactRelationName="fractureContact"
      fluidSolverName="SinglePhaseFlow"
      couplingTypeOption="FIM"
      discretization="FE1"
      targetRegions="{Region2, Fracture}">
      <NonlinearSolverParameters
        newtonMaxIter="50"
        lineSearchMaxCuts="10"
        newtonTol="1.0e-5"/>
      <LinearSolverParameters
        logLevel="0"
        solverType="gmres"
        preconditionerType="amg"/>
    </Hydrofracture>

    <SolidMechanicsLagrangianSSLE
      timeIntegrationOption="QuasiStatic"
      discretization="FE1"
      name="lagsolve"
      logLevel="0"
      contactRelationName="fractureContact"
      solidMaterialNames="{rock}"
      targetRegions="{Region2}">
      <NonlinearSolverParameters
        newtonMaxIter="5"
        newtonTol="1.0e-6"/>
      <LinearSolverParameters
        logLevel="0"
        krylovTol="1.0e-10"
        solverType="gmres"/>
    </SolidMechanicsLagrangianSSLE>

    <SinglePhaseFVM
      discretization="singlePhaseTPFA"
      name="SinglePhaseFlow"
      solidNames="{rock}"
      logLevel="0"
      fluidNames="{water}"
      meanPermCoeff="0.8"
      targetRegions="{Fracture}">
      <NonlinearSolverParameters
        newtonMaxIter="10"
        newtonTol="1.0e-5"/>
      <LinearSolverParameters
        logLevel="0"
        krylovTol="1.0e-12"
        solverType="gmres"/>
    </SinglePhaseFVM>

    <SurfaceGenerator
      name="SurfaceGen"
      logLevel="0"
      fractureRegion="Fracture"
      targetRegions="{ Region2 }"
      solidMaterialNames="{ rock }"
      rockToughness="$K_upscaled$"
      nodeBasedSIF="1"
      mpiCommOrder="1"/>
  </Solvers>

  <Outputs>
    <Silo
      parallelThreads="16"
      plotLevel="3"
      name="siloOutput"/>

    <Restart
      name="restartOutput"/>

    <Python
      name="pythonOutput"/>
  </Outputs>

  <Events
    maxTime="$t_max$">
    <!-- Generate the initial fractures -->
    <SoloEvent
      name="preFracture"
      target="/Solvers/SurfaceGen"/>

    <!-- Primary outputs -->
    <PeriodicEvent
      timeFrequency="1 [min]"
      name="outputs"
      target="/Outputs/siloOutput"/>

    <!-- Apply the hydrofracture solver, limiting the timesteps during certain time intervals -->
    <PeriodicEvent
      endTime="$t_init_a$"
      maxEventDt="$dt_max_a$"
      name="solverApplications_a"
      target="/Solvers/hydrofracture"/>

    <PeriodicEvent
      endTime="$t_init_b$"
      maxEventDt="$dt_max_b$"
      target="/Solvers/hydrofracture"
      beginTime="$t_init_a$"
      name="solverApplications_b"/>

    <PeriodicEvent
      maxEventDt="$dt_max_c$"
      target="/Solvers/hydrofracture"
      beginTime="$t_init_b$"
      name="solverApplications_c"/>

    <!-- Watch the wall-clock, write a restart, and exit gracefully if necessary -->
    <HaltEvent
      maxRuntime="$t_allocation$"
      name="restarts"
      target="/Outputs/restartOutput"/>

    <!-- Python landing-point -->
    <PeriodicEvent
      name="python"
      cycleFrequency="10"
      target="/Outputs/pythonOutput"/>
  </Events>

  <ElementRegions>
    <CellElementRegion
      materialList="{water, rock}"
      cellBlocks="{cb1}"
      name="Region2"/>

    <FaceElementRegion
      materialList="{water, rock}"
      defaultAperture="1.0e-4"
      name="Fracture"/>
  </ElementRegions>

  <Constitutive>
    <CompressibleSinglePhaseFluid
      referenceDensity="1000"
      name="water"
      viscosibility="0.0"
      defaultViscosity="0.001"
      referenceViscosity="1.0e-3"
      referencePressure="0.0"
      compressibility="5e-10"
      defaultDensity="1000"/>

    <PoroLinearElasticIsotropic
      name="rock"
      compressibility="1.6155088853e-18"
      defaultShearModulus="1.0e9"
      defaultDensity="2700"
      defaultBulkModulus="1.0e9"
      BiotCoefficient="1"
      referencePressure="2.125e6"/>

    <Contact
      penaltyStiffness="0.0e8"
      name="fractureContact">
      <TableFunction
        values="{  1.0e-6, 1.0e-4}"
        name="aperTable"
        coordinates="{ -1.0e-3, 0.0}"/>
    </Contact>
  </Constitutive>


  <NumericalMethods>
    <FiniteElements>
      <FiniteElementSpace
        name="FE1"
        order="1"/>
    </FiniteElements>

    <FiniteVolume>
      <TwoPointFluxApproximation
        name="singlePhaseTPFA"
        fieldName="pressure"
        coefficientName="permeability"/>
    </FiniteVolume>
  </NumericalMethods>

  <Functions>
    <!-- Pumping Schedule -->
    <TableFunction
      inputVarNames="{time}"
      voxelFile="$table_root$/flowRate.csv"
      coordinateFiles="{$table_root$/flowRate_time.csv}"
      name="flow_rate"
      interpolation="linear"/>

    <!-- Geologic Model -->
    <TableFunction
      inputVarNames="{elementCenter}"
      voxelFile="$table_root$/sigma_xx.csv"
      coordinateFiles="{$table_root$/x.csv, $table_root$/y.csv, $table_root$/z.csv}"
      name="sigma_xx"
      interpolation="nearest"/>

    <TableFunction
      inputVarNames="{elementCenter}"
      voxelFile="$table_root$/sigma_yy.csv"
      coordinateFiles="{$table_root$/x.csv, $table_root$/y.csv, $table_root$/z.csv}"
      name="sigma_yy"
      interpolation="nearest"/>

    <TableFunction
      inputVarNames="{elementCenter}"
      voxelFile="$table_root$/sigma_zz.csv"
      coordinateFiles="{$table_root$/x.csv, $table_root$/y.csv, $table_root$/z.csv}"
      name="sigma_zz"
      interpolation="nearest"/>

    <TableFunction
      inputVarNames="{elementCenter}"
      voxelFile="$table_root$/porePressure.csv"
      coordinateFiles="{$table_root$/x.csv, $table_root$/y.csv, $table_root$/z.csv}"
      name="init_pressure"
      interpolation="nearest"/>

    <TableFunction
      inputVarNames="{elementCenter}"
      voxelFile="$table_root$/bulkModulus.csv"
      coordinateFiles="{$table_root$/x.csv, $table_root$/y.csv, $table_root$/z.csv}"
      name="bulk_modulus"
      interpolation="nearest"/>

    <TableFunction
      inputVarNames="{elementCenter}"
      voxelFile="$table_root$/shearModulus.csv"
      coordinateFiles="{$table_root$/x.csv, $table_root$/y.csv, $table_root$/z.csv}"
      name="shear_modulus"
      interpolation="nearest"/>
  </Functions>

  <FieldSpecifications>
    <FieldSpecification
      scale="1.0"
      fieldName="rock_BulkModulus"
      name="bulk_modulus"
      setNames="{all}"
      objectPath="ElementRegions"
      initialCondition="1"
      functionName="bulk_modulus"/>

    <FieldSpecification
      scale="1.0"
      fieldName="rock_ShearModulus"
      name="shear_modulus"
      setNames="{all}"
      objectPath="ElementRegions"
      initialCondition="1"
      functionName="shear_modulus"/>

    <FieldSpecification
      scale="1.0"
      name="sigma_xx"
      objectPath="ElementRegions"
      initialCondition="1"
      component="0"
      fieldName="rock_stress"
      setNames="{all}"
      functionName="sigma_xx"/>

    <FieldSpecification
      scale="1.0"
      name="sigma_yy"
      objectPath="ElementRegions"
      initialCondition="1"
      component="1"
      fieldName="rock_stress"
      setNames="{all}"
      functionName="sigma_yy"/>

    <FieldSpecification
      scale="1.0"
      name="sigma_zz"
      objectPath="ElementRegions"
      initialCondition="1"
      component="2"
      fieldName="rock_stress"
      setNames="{all}"
      functionName="sigma_zz"/>

    <!-- Mechanical BC's -->
    <FieldSpecification
      scale="0.0"
      fieldName="TotalDisplacement"
      name="x_constraint"
      setNames="{xneg, xpos}"
      objectPath="nodeManager"
      component="0"/>

    <FieldSpecification
      scale="0.0"
      fieldName="TotalDisplacement"
      name="y_constraint"
      setNames="{yneg, ypos}"
      objectPath="nodeManager"
      component="1"/>

    <FieldSpecification
      scale="0.0"
      fieldName="TotalDisplacement"
      name="z_constraint"
      setNames="{zneg, zpos}"
      objectPath="nodeManager"
      component="2"/>
  </FieldSpecifications>
</Problem>
